            $(PdfTemplateHtml).find(".eduqualifygrid").each(function () {
                $(this).find("table").each(function () {
                    $(this).find("tbody th").css("border-top-width", "0.1px");
                    $(this).find("tbody th").css("border-left-width", "0.1px");
                    $(this).find("tbody td").css("border-top-width", "0.1px");
                    $(this).find("tbody td").css("border-left-width", "0.1px");
                    $(this).find("tbody tr").each(function () {
                        $(this).find("td:last").css("border-right-width", "0.1px");
                        $(this).find("th:last").css("border-right-width", "0.1px");
                    });

                    $(this).find("tbody tr:last").each(function () {
                        $(this).find("td").css("border-bottom-width", "0.1px");
                    });
                });
                $(this).find("ul").each(function () {
                    var text = '<table style="width: 100%;"><tbody><tr><td style="padding-left:-10px;padding-top:-5px;"><table style="width: 100%;" class="requirement-ul"><tbody>';
                    $.each($(this).find(">li"), function (i, n) {
                        var innerText = $(this).contents()[0].data || $(this).find(":first-child").clone().wrap('<p>').parent().html();
                        text = text + "<tr><td style=\"width: 2.5%;\"><font style=\"font-size: 20px;\">&#9642;</font></td><td style=\"width: 97.5%;\">" + innerText + "</td></tr>";
                    });
                    $(this).replaceWith(text + "</tbody></table></td></tr></tbody></table>");
                });
            });
	        $(PdfTemplateHtml).find(".ComparisonGrid").each(function () {
	            $(this).find("table").each(function () {
	                $(this).find("tbody th").css("border-top-width", "0.1px");
	                $(this).find("tbody th").css("border-left-width", "0.1px");
	                $(this).find("tbody td").css("border-top-width", "0.1px");
	                $(this).find("tbody td").css("border-left-width", "0.1px");
	                $(this).find("tbody tr").each(function () {
	                    $(this).find("td:last").css("border-right-width", "0.1px");
	                    $(this).find("th:last").css("border-right-width", "0.1px");
	                });

	                $(this).find("tbody tr:last").each(function () {
	                    $(this).find("td").css("border-bottom-width", "0.1px");
	                });
	            });
	            $(this).find(".comparisonContent ul").each(function () {
	                var text = '<table style="width: 100%;"><tbody><tr><td style="padding-left:0px;padding-top:-7px;"><table style="width: 100%;" class="comparisonContent-table"><tbody>';
	                $.each($(this).find(">li"), function (i, n) {
	                    var innerText = $(this).contents()[0].data || $(this).find(":first-child").clone().wrap('<p>').parent().html();
	                    text = text + "<tr><td style=\"width:8%;\"><font style=\"font-size: 20px;\">&#9642;</font></td><td style=\"width: 92%;\">" + innerText + "</td></tr>";
	                });
	                $(this).replaceWith(text + "</tbody></table></td></tr></tbody></table>");
	            });
	        });


            $(PdfTemplateHtml).find("#tableDragger .ul-box:empty").remove();
            $(PdfTemplateHtml).find("#tableDragger .ol-box:empty").remove();
            $(PdfTemplateHtml).find("#tableDragger .ul-alpha-box:empty").remove();
            $(PdfTemplateHtml).find("#tableDragger .ul-hyp-box:empty").remove();

            $(PdfTemplateHtml).find(".ol-box").each(function () {
                if ($(this).find(">li").length == 0)
                    $(this).remove();
                if (!$(this).parent().is("li")) {

                    var firstTdWidth = $(this).hasClass("guidlisce") ? '3%' : '6%';
                    var secondTdWidth = $(this).hasClass("guidlisce") ? '97%' : '96%';
                    var className = $(this).hasClass("guidlisce") ? "guidlisce" : "Procedure";
                    var text = '<table style=\"width:100%\" class="list-control-table ' + className + '"><tbody>';
                    var resetCount = 1;
                    var IsnumberRestart = $(this).css("counter-increment").split(' ')[1] == undefined ? "" : parseInt($(this).css("counter-increment").split(' ')[1]);

                    $.each($(this).find(">li"), function (i, n) {
                        var counter = IsnumberRestart != "" ? (IsnumberRestart + resetCount) : (i + 1);
                        var innerText = $(this).find(":first-child").length ? CommonFunc.DownloadPdf._GetData($(this)) : $(this).contents()[0].data;
                        text = text + "<tr><td width='" + firstTdWidth + "'>" + counter + ")</td><td width='" + secondTdWidth + "'>" + innerText + "</td></tr>";
                        text = CommonFunc.DownloadPdf._TraverseList(text, $(this));
                        ++resetCount;

                    });
                    $(this).replaceWith(text + "</tbody></table>");
                }
            });
            $(PdfTemplateHtml).find(".ul-alpha-box").each(function () {
                if ($(this).find(">li").length == 0)
                    $(this).remove();
                if (!$(this).parent().is("li")) {

                    var firstTdWidth = $(this).hasClass("guidlisce") ? '3%' : '5.5%';
                    var secondTdWidth = $(this).hasClass("guidlisce") ? '97%' : '94.5%';
                    var className = $(this).hasClass("guidlisce") ? "guidlisce" : "Procedure";
                    var childCount = 97;
                    var text = '<table style=\"width: 100%;\" class="list-control-table ' + className + '"><tbody>';
                    if ($(this).hasClass("sub-alpha-list"))
                        text = '<table style=\"width: 100%;\"><tbody><tr><td class="ul-alpha-box-postion"><table style=\"width:100%;\" class="list-control-table ' + className + '"><tbody>';

                    var IsnumberRestart = $(this).css("counter-increment").split(' ')[1] == undefined ? 0 : parseInt($(this).css("counter-increment").split(' ')[1]);
                    childCount = childCount + IsnumberRestart;

                    $.each($(this).find(">li"), function (i, n) {
                        var innerText = $(this).find(":first-child").length ? CommonFunc.DownloadPdf._GetData($(this)) : $(this).contents()[0].data;
                        text = text + "<tr><td width='" + firstTdWidth + "'>" + String.fromCharCode(childCount) + ")</td><td width='" + secondTdWidth + "'>" + innerText + "</td></tr>";
                        text = CommonFunc.DownloadPdf._TraverseList(text, $(this));
                        ++childCount;
                    });
                    if (!$(this).hasClass("sub-alpha-list"))
                        $(this).replaceWith(text + "</tbody></table>");
                    else $(this).replaceWith(text + "</tbody></table></td></tr></tbody></table>");
                }
            });
            $(PdfTemplateHtml).find(".ul-box").each(function () {
                if ($(this).find(">li").length == 0)
                    $(this).remove();
                if (!$(this).parent().is("li")) {

                    var firstTdWidth = $(this).hasClass("guidlisce") ? '3%' : '5.5%';
                    var secondTdWidth = $(this).hasClass("guidlisce") ? '97%' : '94.5%';
                    var className = $(this).hasClass("guidlisce") ? "guidlisce" : "Procedure";
                    var padding = $(this).hasClass("guidlisce") ? "padding-top: -7px;" : "padding-top: -5px;";
                    var text = '<table style=\"width: 100%;\" class="list-control-table ' + className + '"><tbody>';
                    if ($(this).hasClass("sub-disc-list"))
                        text = '<table style=\"width: 100%;\"><tbody><tr><td class="ul-box-postion"><table style=\"width: 100%;\" class="list-control-table ' + className + '"><tbody>';

                    $.each($(this).find(">li"), function (i, n) {
                        var innerText = $(this).find(":first-child").length ? CommonFunc.DownloadPdf._GetData($(this)) : $(this).contents()[0].data;
                        text = text + "<tr><td width='" + firstTdWidth + "' style='" + padding + "'><font style=\"font-size: 10px;\" face=\"Wingdings\">&#108;</font></td><td width='" + secondTdWidth + "'>" + innerText + "</td></tr>";
                        text = CommonFunc.DownloadPdf._TraverseList(text, $(this));
                    });
                    if (!$(this).hasClass("sub-disc-list"))
                        $(this).replaceWith(text + "</tbody></table>");
                    else $(this).replaceWith(text + "</tbody></table></td></tr></tbody></table>");
                }
            });
            $(PdfTemplateHtml).find(".ul-hyp-box").each(function () {
                if ($(this).find(">li").length == 0)
                    $(this).remove();
                if (!$(this).parent().is("li")) {

                    var firstTdWidth = $(this).hasClass("guidlisce") ? '3%' : '5.5%';
                    var secondTdWidth = $(this).hasClass("guidlisce") ? '97%' : '94.5%';
                    var className = $(this).hasClass("guidlisce") ? "guidlisce" : "Procedure";
                    var padding = $(this).hasClass("guidlisce") ? "padding-top: -7px;" : "padding-top: -5px;";

                    var text = '<table style=\"width: 100%;\" class="list-control-table ' + className + '"><tbody>';
                    if ($(this).hasClass("sub-hypen-list"))
                        text = '<table style=\"width: 100%;\"><tbody><tr><td class="ul-hyp-box-postion"><table style=\"width: 100%;\" class="list-control-table ' + className + '"><tbody>';

                    $.each($(this).find(">li"), function (i, n) {
                        var innerText = $(this).find(":first-child").length ? CommonFunc.DownloadPdf._GetData($(this)) : $(this).contents()[0].data;
                        text = text + "<tr><td width='" + firstTdWidth + "' style='" + padding + "'><font style=\"font-size: 10px;\">&#8211;</font></td><td width='" + secondTdWidth + "'>" + innerText + "</td></tr>";
                        text = CommonFunc.DownloadPdf._TraverseList(text, $(this));
                    });
                    if (!$(this).hasClass("sub-hypen-list"))
                        $(this).replaceWith(text + "</tbody></table>");
                    else $(this).replaceWith(text + "</tbody></table></td></tr></tbody></table>");
                }
            });

	        CommonFunc.DownloadPdf._removeUnwantedTags($(PdfTemplateHtml));
            return $(PdfTemplateHtml).html().replace(/&lt;/gi, "_LessthanOperator");
    },
	_removeUnwantedTags:function(control)
	{
	    control.find(".drg-rmv").remove();
	    control.find(".templateSettings").remove();
	    control.find(".drg-rmv-settings").remove();
	    control.find(".drg-rmv-settings-toc").remove();
	    control.find(".drg-rmv-toc").remove();
	    control.find(".drg-rmv-copy").remove();
	    control.find(".drg-rmv-settings-copy").remove();
	    control.find(".drg-rmv-toc-copy").remove();
	    control.find(".drg-rmv-settings-toc-copy").remove();
	    control.find(".drg-rmv-addrow-deleterow-toc-copy").remove();
	    control.find(".drg-rmv-addrow-deleterow-withsubrow").remove();
	    control.find(".pop-overlay").remove();
        control.find(".bookmark-settings").remove();
        control.find(".list-numbering-box").remove();
        control.find("grammarly-btn").remove();
	},
	_TraverseList: function (text, control) {
	    var firstTdWidth = $("#pageSize").val() == "A4" ? '3%' : '5.5%';
	    var secondTdWidth = $("#pageSize").val() == "A4" ? '97%' : '94.5%';
	    if (control.contents().length > 1) {
	        text = text + "<tr><td colspan=\"2\" class=\"inner-list-Postion\">";
	        control.contents().each(function () {
	            if ($(this).is(".ul-alpha-box")) {
	                var childCount = 97;
	                text = text + "<table style=\"width: 100%;\" class='inner-table'><tbody>";
	                $.each($(this).find(">li"), function () {
                        var innerText = $(this).find(":first-child").length ? CommonFunc.DownloadPdf._GetData($(this)) : $(this).contents()[0].data;
	                    text = text + "<tr><td width='" + firstTdWidth + "'>" + String.fromCharCode(childCount) + ")</td><td width='" + secondTdWidth + "'>" + innerText + "</td></tr>";
	                    text=CommonFunc.DownloadPdf._TraverseList(text, $(this));
	                    ++childCount;
	                });
	                text = text + "</tbody></table>";
	            }
                else if ($(this).is(".ol-box")) {
                     firstTdWidth = $("#pageSize").val() == "A4" ? '3%' : '6%';
                     secondTdWidth = $("#pageSize").val() == "A4" ? '97%' : '96%';

	                text = text + "<table style=\"width: 100%;\" class='inner-table'><tbody>";
	                $.each($(this).find(">li"), function (j, k) {
                        var innerText = $(this).find(":first-child").length ? CommonFunc.DownloadPdf._GetData($(this)) : $(this).contents()[0].data;
	                    text = text + "<tr><td width='" + firstTdWidth + "'>" + (j + 1) + ")</td><td width='" + secondTdWidth + "'>" + innerText + "</td></tr>";
	                    text=CommonFunc.DownloadPdf._TraverseList(text, $(this));
	                });
	                text = text + "</tbody></table>";
	            }
	            else if ($(this).is(".ul-box") || $(this).is(".ul-hyp-box") || $(this).is(".ul-hyp-small-box")) {
	                var numbering = $(this).is(".ul-box") ? "<font style=\"font-size: 10px;text-align: center\" face=\"Wingdings\">&#108;</font>" : "<font style=\"font-size: 10px;text-align: center\">&#8211;</font>";
	                var classname = $(this).is(".ul-hyp-small-box") ? 'inner-table small-hyphen' : 'inner-table';
	                var padding = $("#pageSize").val()=="A4" ? "padding-top: -7px;" : "padding-top: -5px;";
	                text = text + "<table style=\"width: 100%;\" class='" + classname + "'><tbody>";
	                $.each($(this).find(">li"), function () {
                        var innerText = $(this).find(":first-child").length ? CommonFunc.DownloadPdf._GetData($(this)) : $(this).contents()[0].data;
	                    text = text + "<tr><td width='" + firstTdWidth + "' style='" + padding + "'>" + numbering + "</td><td width='" + secondTdWidth + "'>" + innerText + "</td></tr>";
	                    text=CommonFunc.DownloadPdf._TraverseList(text, $(this));
	                });
	                text = text + "</tbody></table>";
	            }
	        });
	        text = text + "</td></tr>";
	    }
	    return text;
    },
	_AddFontSize: function (control) {
	    var size = 0;
        if (control.find(":first").is("font")) {
            size = parseInt(control.find(":first").attr("size"));
            if (isNaN(size)) {
                size =parseInt(control.find(":first").attr("style").split(':')[1].trim().replace("px;", ""));
            }
            else return "font-size:" + CommonFunc.DownloadPdf._SetPixel(size) * 72 / 96 + "pt !important";
        }
        if (size == 0)
            return "";
    },
	_SetPixel: function (size) {
	    switch(size){
	        case 1: {
	            return 10;
	            break;
	        }
	        case 2: {
	            return 13;
	            break;
	        } case 3: {
	            return 16;
	            break;
	        } case 4: {
	            return 18;
	            break;
	        } case 5: {
	            return 24;
	            break;
	        }
	        case 6: {
	            return 32;
	            break;
	        }
	        case 7: {
	            return 48;
	            break;
	        }
}
    },
    _GenerateHtml: function (templateHtml) {

    templateHtml.find("[contenteditable]").removeAttr("contenteditable");
    templateHtml.find(".pagebreak").css("page-break-after", "always").html("<td></td>");
    templateHtml.find("#tableDragger").children().find("table.draggable-item").addClass("wikidoctemplate");
    templateHtml.find("#imgcontainer").unwrap("div");
    templateHtml.find("#tableDragger").find(".comment-box").remove();
    templateHtml.find("#tableDragger").find("script").remove();

    //if (templateHtml.find(".headerTemplate:first").attr("data-column") == "column-PageHeader-BSM") {
    //    var pagehead = templateHtml.find(".headerTemplate:first").find(".header-text").text();
    //    templateHtml.find(".headerTemplate:first").find(".insertMeidum").html("<div style=\"background:#007ac9;color: #fff; padding: 6px;\"><table style=\"width:100%;\"><tbody><tr><td class=\"page-heading\">" + pagehead+"</td><td class=\"page-headingBSM\">BSM</td></tr></tbody></table></div>");
    //}
    templateHtml.find(".htmlpreview").each(function () {
        $(this).closest(".draggable-item").html($(this).html());
    });
    templateHtml.find("#tableDragger").find(".toggle-article").remove();
    templateHtml.find("#tableDragger .article-table").each(function () {
        $(this).find(">tbody>tr").remove();
    });

    templateHtml.find(".bookletDocLeft").removeClass("bookletDocLeft");
    templateHtml.find(".bookletDocRight").removeClass("bookletDocRight");

    templateHtml.find(".filetext").each(function () { if ($(this).attr("onclick").includes("RenderLinkDocView") || $(this).attr("onclick").includes("RenderLinkArticleView")) { $(this).parent().text($(this).parent().text()); $(this).remove(); } });

    templateHtml.find(".SubHeader1").each(function () {
        if ($(this).closest("td").attr("style") == undefined) {
            $(this).closest("td").closest("table.draggable-item").removeAttr("height");
            $(this).removeAttr("style");
        }
    });
    templateHtml.find(".subheaderText").each(function () {
        $(this).closest("td").closest("table.draggable-item").removeAttr("height");
    });

    templateHtml.find("#tableDragger .section-class-one").each(function () {
        if ($(this).find("ol").length) {
            $(this).find("ol").attr("style", $(this).attr("style"));
            $(this).removeAttr("style");
        }
        if ($(this).find("ul").length) {
            $(this).find("ul").attr("style", $(this).attr("style"));
            $(this).removeAttr("style");
        }
    });
    templateHtml.find("#tableDragger .singleCol-text").each(function () {
        if ($(this).find("ol").length) {
            $(this).find("ol").attr("style", $(this).attr("style"));
            $(this).removeAttr("style");
        }
        if ($(this).find("ul").length) {
            $(this).find("ul").attr("style", $(this).attr("style"));
            $(this).removeAttr("style");
        }
    });
    templateHtml.find("#tableDragger").find("[data-column='column-completed-layout']").each(function () {
        var html = '<table style=\"width:100%;\" class="completed-table"><tbody><tr><td class="box-td" align="right"> <span style="font-family:\'Wingdings 2\';font-size:25px">ï¿½</span><span style="font-family: \'Wingdings 2\';font-size:25px">ï¿½</span></td><td class="completed-td">Completed</td><td class="box-td"> <span style="font-family: \'Wingdings 2\';font-size:25px">ï¿½</span><span style="font-family: \'Wingdings 2\';font-size:25px">ï¿½</span></td></tr></tbody></table>';
        $(this).find(".completed-svg").replaceWith(html);
        $(this).find("table.draggable-item").attr("height", 27);
    });

    templateHtml.find("table[id^=resizeTable]").each(function () {
        $(this).removeAttr("border");
        $(this).css("width", $(this).attr("actual-width"));
        $(this).find(".resizeCol").each(function () {
            if ($(this).width() > 50)
                $(this).css("width", $(this).attr("actual-width"));
        });
        $(this).find(".ui-resizable-handle").remove();

        var tdBorder = $(this).find(">tbody>tr>td:eq(0)").css("border-left") != undefined && $(this).find(">tbody>tr>td:eq(0)").css("border-left") != "" ? $(this).find(">tbody>tr>td:eq(0)").css("border-left").trim().split(' ')[0] : "0.1px";
        var border = $(this).attr("border") != undefined && $(this).attr("border") != "" ? $(this).attr("border") : tdBorder;
        var thBorder = $(this).find(">thead th:eq(0)").css("border-bottom") != undefined && $(this).find(">thead th:eq(0)").css("border-bottom") != "" ? $(this).find(">thead th:eq(0)").css("border-bottom").trim().split(' ')[0] : "0.1px";

        $(this).removeAttr("cellpadding");
        $(this).removeAttr("cellspacing");


        $(this).find("thead th").each(function () {
            if ($(this).css("height") != undefined && $(this).css("height") != "")
                $(this).attr("style", "width:" + $(this).css("width") + ";height:" + $(this).css("height"));
            else $(this).attr("style", "width:" + $(this).css("width"));
        });

        $(this).find("thead th").css("border-top-width", thBorder != "0.1px" ? thBorder : border);
        $(this).find("thead th:first").css("border-left-width", thBorder != "0.1px" ? thBorder : border);
        $(this).find("thead th:not(:first)").css("border-left-width", "0.1px");
        $(this).find("thead th:last").css("border-right-width", thBorder != "0.1px" ? thBorder : border);
        $(this).find(".ui-resizable-handle").remove();

        $(this).find(">tbody>tr").each(function (tr,trs) {
            var isFirstHiddenCell = false;
            $(this).find(">td").each(function (i, n) {

                var IsHiddenCell = $(this).css("border");
                $(this).css("border", "");
                $(this).attr("style", "width:" + $(this).css("width"));

                if (IsHiddenCell != "none") {
                    $(this).css("border-top-width", tr == 0 ? thBorder : "0.1px");
                    $(this).css("border-left-width", (i == 0 ? border != "0.1px" ? border : "0.1px" : "0.1px"));
                    if (i == $(this).parent().find(">td").length - 1)
                        $(this).css("border-right-width", border != "0.1px" ? border : "0.1px");
                }
                else if (!isFirstHiddenCell) {
                    isFirstHiddenCell = true;
                    $(this).css("border-left-width", "0.1px");
                }
            });
        });
        $(this).find(">tbody>tr:last").find(">td").each(function () {
            var IsHiddenCell = $(this).css("border");
            if (IsHiddenCell != "none")
                $(this).css("border-bottom-width", border != "0.1px" ? border : "0.1px");
        });

        if ($(this).closest(".draggable-item").closest("tr").prev().find("table.draggable-item>tbody").find(".ol-box").length) {
            if ($(this).closest(".draggable-item").closest("tr").prev().find("table.draggable-item>tbody").find(".ol-box:last").nextAll().text().length == 0)
                $(this).closest(".draggable-item").css("width", "96%");
            $(this).closest(".insertMeidum").attr("style", "padding-top:-5px");
        }
        else if ($(this).closest(".draggable-item").closest("tr").prev().find("table.draggable-item>tbody").find(".list-control-table").length) {
            if ($(this).closest(".draggable-item").closest("tr").prev().find("table.draggable-item>tbody").find(".list-control-table:last").nextAll().text().length == 0)
                $(this).closest(".draggable-item").css("width", "96%");
            $(this).closest(".insertMeidum").attr("style", "padding-top:-5px");
        }

        $(this).closest(".draggable-item").closest("tr").find(".sub-disc-list").each(function () {
            $(this).removeClass("sub-disc-list");
        });
    });
    templateHtml.find(".policy-table").each(function () {
        $(this).removeAttr("border");
        $(this).find("thead th").css("border-top-width", "0.1px");
        $(this).find("thead th").css("border-left-width", "0.1px");
        $(this).find("thead th:last").css("border-right-width", "0.1px");
        $(this).find(".ui-resizable-handle").remove();
        $(this).find("tbody td").css("border-top-width", "0.1px");
        $(this).find("tbody td").css("border-left-width", "0.1px");
        $(this).find("tbody tr").each(function () {
            $(this).find("td:last").css("border-right-width", "0.1px");
        });
        $(this).find("tbody tr:last").each(function () {
            $(this).find("td").css("border-bottom-width", "0.1px");
        });
        $(this).find("tbody tr:eq(1)").find("td:first").css("border-bottom-width", "0.1px");
    });
    templateHtml.find(".pcgridview").each(function () {
        $(this).find("table").each(function () {
            $(this).find("tbody th").css("border-top-width", "0.1px");
            $(this).find("tbody th").css("border-left-width", "0.1px");
            $(this).find("tbody td").css("border-top-width", "0.1px");
            $(this).find("tbody td").css("border-left-width", "0.1px");
            $(this).find("tbody tr").each(function () {
                $(this).find("td:last").css("border-right-width", "0.1px");
                $(this).find("th:last").css("border-right-width", "0.1px");
            });

            $(this).find("tbody tr:last").each(function () {
                $(this).find("td").css("border-bottom-width", "0.1px");
            });
        });
    });
    templateHtml.find(".rpgridview").each(function () {
        $(this).find("table").each(function () {
            $(this).find("tbody td").css("border-top-width", "0.1px");
            $(this).find("tbody td").css("border-left-width", "0.1px");
            $(this).find("tbody tr").each(function () {
                $(this).find("td:last").css("border-right-width", "0.1px");
            });

            $(this).find("tbody tr:last").each(function () {
                $(this).find("td").css("border-bottom-width", "0.1px");
            });
        });
    });
    templateHtml.find(".rpngridview").each(function () {
        $(this).find("table").each(function () {
            $(this).find("tbody td").css("border-top-width", "0.1px");
            $(this).find("tbody td").css("border-left-width", "0.1px");
            $(this).find("tbody tr").each(function () {
                $(this).find("td:last").css("border-right-width", "0.1px");
            });

            $(this).find("tbody tr:last").each(function () {
                $(this).find("td").css("border-bottom-width", "0.1px");
            });
        });
    });
    templateHtml.find(".jdgridview").each(function () {
        $(this).find("table").each(function () {
            $(this).find("tbody td").css("border-top-width", "0.1px");
            $(this).find("tbody td").css("border-left-width", "0.1px");
            $(this).find("tbody tr").each(function () {
                $(this).find("td:last").css("border-right-width", "0.1px");
            });

            $(this).find("tbody tr:last").each(function () {
                $(this).find("td").css("border-bottom-width", "0.1px");
            });
        });

    });
    templateHtml.find(".jobdetailsgridview").each(function () {
        $(this).find("table").each(function () {
            $(this).find("tbody th").css("border-top-width", "0.1px");
            $(this).find("tbody th").css("border-left-width", "0.1px");
            $(this).find("tbody td").css("border-top-width", "0.1px");
            $(this).find("tbody td").css("border-left-width", "0.1px");
            $(this).find("tbody tr").each(function () {
                $(this).find("td:last").css("border-right-width", "0.1px");
                $(this).find("th:last").css("border-right-width", "0.1px");
            });

