            $(".viewdoc").scroll(function () {

                var viewRangeStart = $(this).offset().top;
                var viewRangeEnd = viewRangeStart + $(this).height();
                var eleTop = $(this).offset().top;
                var eleBottom = eleTop + $(this).height();

                $(this).find(".article-table[data-visible='false']").filter(function () {
                    var eleTop = $(this).offset().top;
                    var eleBottom = eleTop + $(this).height();
                    if ((eleBottom <= viewRangeEnd) && (eleTop >= viewRangeStart)) {

                        if ($(this).attr("data-visible") == "false") {
                            $(this).attr("data-visible", true);
                           ArticleData($(this), $("#IsDCRLCycle").val(), true, $("#Id").val())
                             //  ArticleData($(this), $("#IsDCRLCycle").val(), true, $("#Id").val());
                           // GetAllArticlesData($(this));
                        }
                    }

                });


            });
        }


        setTimeout(function () {
            $("#header_search_field").val("");
            //$("body").removeClass();
            $(".aLwRaPeR").remove();
            $(".list-numbering-box").remove();

            $('#tableDragger').find(".bookmark-settings").html(CommonFunc.Bookmark._bookmarkSettings());
            $('#tableDragger').find(".bookmark-settings").show();

            $("body").stop().addClass("docInfo-panel-close");
            $("#template-tools-tab").find(".tab-1, .tab-2").removeClass("active");
            $(".htmlpreview").each(function () {
                $(this).closest(".draggable-item").html($(this).html());
            });

            $("#tableWrapper").find('table').removeClass('NewDataArt');
            $(".toggle-article").click(function () {
                $(this).closest("tr.draggable-item").find(".article-table").attr("data-visible", true);
                $(this).toggleClass('opentoggle');
                if ($(this).closest("tr.draggable-item").find(".article-table").hasClass("NewDataArt") == false) {
                    var loaderNew = $('body').find('.loadernew')[0]
                    if (!$(this).closest("tr.draggable-item").find(".article-table").find(".loadernew").length)
                        $(this).closest("tr.draggable-item").find(".article-table").find('.insertMeidum h1.subheaderText').append(loaderNew);
                    ArticleData($(this).closest("tr.draggable-item").find(".article-table"), $("#IsDCRLCycle").val(), false, $("#Id").val());
                }

            });
            $(".singleCol-text").each(function () {
                if ($(this).next().is("ol") || $(this).next().is("ul"))
                    $(this).next().appendTo($(this));
            });
            $(".section-class-one").each(function () {
                if ($(this).next().is("ol") || $(this).next().is("ul"))
                    $(this).next().appendTo($(this));
            });
            //$("#tableDragger").find(".blankPage").each(function () {

            //    $(this).replaceWith('<tr data-column="column-pagebreak-layout" class="pagebreak"> <td style=" padding:10px 0px;display: block; background-color: transparent; "> <table align="center" style="width:100%" border="0" cellpadding="0" cellspacing="0" class="draggable-item ui-state-default"> <tbody> <tr> <td> <div class="drg-rmv"></div><p></p></td></tr></tbody> </table> </td></tr>');
            //});
            $("#tableDragger").find(".blankPage").remove();
            $(".thumb-wrapper").find(".ui-resizable-handle").hide();
            $('img').on('dragstart', function (event) { event.preventDefault(); });
            $("table[id^=resizeTable]").each(function () {
                if ($(this).css("border-bottom").length)
                    $(this).css("border-bottom", "");
            });

        }, 100);
    },
    SetDocDetails: function (result) {
        
        $(".loadernew").remove();
        
        //$("ul.documentFields").append(LoaderHtml());
        PublishedDoc = result.IsPublished;
        if (result.IsValid == true) {
            if (result.IsAckSubmit == false && result.IsDocAckEnabled) {

                $('#Acknlwdge').fadeIn();
                if ($('#Acknlwdge').attr("onclick") == "CheckAcknowledgementStatus()")
                    $('#Acknlwdge').attr("onclick", 'Warning()');
                //$("#Acknlwdge").css("pointer-events", "none");
            }

            $(".control-sidebar-tabs2").addClass("show-approvel-data");
            $(".show-approvel-data #RejectedTab").show();
            $(".show-approvel-data #PendingTab").show();
            $(".show-approvel-data #EditedTab").show();

            if (result.IsDocAckEnabled) {
                if (result.Isack) {
                    //if (result.IsInAckDate) {

                    //    $("#IsSubmitAck").val("Y");
                    //    // if not in scroll
                    //    if (Math.round($(".viewdoc").innerHeight()) != Math.round($(".viewdoc")[0].scrollHeight)) {
                    //        $('#Acknlwdge').fadeOut();
                    //        $('#Acknlwdged').fadeIn();
                    //    }
                    //}
                    if (result.IsAckSubmit) {
                        $('#Acknlwdge').fadeOut();
                        $('#Acknlwdged').fadeIn();
                    } else {
                        if (Math.round($(".viewdoc")[0].scrollHeight) < 700) {
                            debugger
                            if ($("#IsSubmitAck").val() == "N") {
                                $('#Acknlwdge').attr("onclick", "CheckAcknowledgementStatus()");

                                $('#Acknlwdged').fadeOut();
                            }
                        }


                        else  if (Math.round($(".viewdoc").innerHeight()) == Math.round($(".viewdoc")[0].scrollHeight)) {
                            if ($("#IsSubmitAck").val() == "N") {
                                $('#Acknlwdge').fadeIn();
                                if ($('#Acknlwdge').attr("onclick") == "Warning()")
                                    $('#Acknlwdge').attr("onclick", 'CheckAcknowledgementStatus()');
                                $('#Acknlwdged').fadeOut();
                            }

                        }
                    }
                }
                else {
                    if (Math.round($(".viewdoc").innerHeight()) == Math.round($(".viewdoc")[0].scrollHeight)) {
                        if ($("#IsSubmitAck").val() == "N") {
                            $('#Acknlwdge').fadeIn();
                            if ($('#Acknlwdge').attr("onclick") == "Warning()")
                                $('#Acknlwdge').attr("onclick", 'CheckAcknowledgementStatus()');
                            $('#Acknlwdged').fadeOut();
                        }

                    }

                    if (result.IsAckSubmit) {

                        $("#IsSubmitAck").val("Y");
                        // if not in scroll
                        if (Math.round($(".viewdoc").innerHeight()) == Math.round($(".viewdoc")[0].scrollHeight)) {
                            if ($("#IsSubmitAck").val() == "N") {
                                $('#Acknlwdge').fadeIn();
                                $('#Acknlwdged').fadeOut();
                            }
                        }
                        else {
                            $('#Acknlwdge').fadeOut();
                            $('#Acknlwdged').fadeIn();
                        }
                    }
                }
            }
            if (result.IsPublished == false) {

                $("#container_link").hide();
                $('#Acknlwdge').fadeOut();
                $('#Acknlwdged').fadeOut();
                $("#containerbookmark_link").hide();

                $("#containerDcrl_link").hide();
            }

            if (result.IsPublished) {
                //if (Envset == "O")
                //{
                //    $("#container_link").show();
                //}

                $("#container_link").show();
                $("#containerbookmark_link").show();
                $("#containerDcrl_link").show();
                $(".dcrl-tab-strip ").show();
            }
            if (result.IsPublished && viewMode=== "view") {
                $("#containerDcrl_link").show();
            }


            if (result.IsPublished && viewMode == "view") {

                $("#containerDcrl_link").hide();
            }

            $("ul.documentFields .tiny-loader").remove();
            $(".doc-name").html('<h4 id="templateAcName" title="' + result.DocumentName + '">' + result.DocumentName + '</h4>');
            if (result.IsPublished == true) {
                if (CommonFunc.Pin.isEabled === true) {
                    CommonFunc.Pin.para = result.Id;
                    CommonFunc.Pin.name = result.DocumentName;
                    CommonFunc.Pin.event = "RenderDocView";
                    CommonFunc.Pin._AllPinnedItems(result.Id);
                    CommonFunc.Pin._CurrentPinnedItems(result.Id);
                    CommonFunc.Pin._IsMoreItemExist();
                }
            }

            CommonFunc.DownloadPdf._setHeader($('#tableDragger'), result);
            CommonFunc.DownloadPdf._setFooter($('#tableDragger'), result);
            if (clientName !== null && clientName === "MOL") { 
                setGlobalApprover();
                setReleasebyName();
            }
            WikiDocSettings.DocumentDeatils._bindEvents();
            WikiDocSettings.ViewDocActivites.CheckExistance(result.Id, result.Version);
            if (result.IsPublished == true) {
                WikiDocSettings.ViewDocActivites.AddRecentActivityAsync(result.Id, result.DocumentName);
            }
            if (result.IsPublished == true) {
                var StartTimeInfo = new Date().getTime();
                $("#AcknlgeStartTime").val(StartTimeInfo);
            }

            if (result.Isvisible === false) {
                $('#editTemplate').hide();
                $('#Acknlwdged').fadeOut();
                // $('#Acknlwdge').hide();
                $('#Acknlwdge').fadeOut();
                $("#containerDcrl_link").hide();
            }
        }
        else {
            criticalNotify("The document has already been attended by another user", "", "Warning");
            window.opener.location.reload(true);
            QDMS.Menu.GetReviewerDocuments();
        }
    }
};
WikiDocSettings.ViewDocActivites = {
    CheckExistance: function (id, version) {
        var input = {
            'Id': id,
            'NewVersion': version
        };
        $.ajax({
            url: $("#hfdUrlPath").val() + '/DocumentSettings/CheckExistance',
            type: 'POST',
            data: input,
            success: function () {
            }
        });
    },
    AddRecentActivityAsync: function (id, name) {
        var input = {
            'Id': id,
            'DocumentName': name
        };
        $.ajax({
            url: $("#hfdUrlPath").val() + '/DocumentSettings/AddRecentActivityAsync',
            type: 'POST',
            data: input,
            success: function () {
            }
        });
    },
    AddArticleRecentActivityAsync: function (id, name) {
        var input = {
            'Id': id,
            'ArticleName': name
        };
        $.ajax({
            url: $("#hfdUrlPath").val() + '/DocumentSettings/AddArticleRecentActivityAsync',
            type: 'POST',
            data: input,
            success: function () {
            }
        });
    }
}
WikiDocSettings.EditDoc = {
    GetDetails: function (id) {
        $(".loadernew").remove();
        $.ajax({
            url: $("#hfdUrlPath").val() + '/DocumentSettings/GetDocEditDetails',
            type: 'GET',
            data: { 'id': id },
            success: function (result) {
                $("#IsDocumentNumberIdentity").val(result.IsDocumentNumberIdentity);
                $("#IsFeaturedEnabled").val(result.IsFeaturedEnabled);
                $("#IsRecommendedEnabled").val(result.IsRecommendedEnabled);
                $("#leftpart").prop("disabled", true);
                $("#rightpart").prop("disabled", true);
                if (result.IsEditor || result.IsAdmin || result.IsSmAdmin) {
                    $(".show-approvel-data #RejectedTab").show();
                    $(".show-approvel-data #PendingTab").hide();
                    $(".show-approvel-data #EditedTab").show();
                }
                if (result.IsReviewer || result.IsAdmin || result.IsSmAdmin) {
                    $(".show-approvel-data #RejectedTab").show();
                    $(".show-approvel-data #PendingTab").show();
                    $(".show-approvel-data #EditedTab").show();
                }
                if (result.IsApprover || result.IsAdmin || result.IsSmAdmin) {
                    $(".show-approvel-data #RejectedTab").hide();
                    $(".show-approvel-data #PendingTab").show();
                    $(".show-approvel-data #EditedTab").show();
                    $("#leftpart").prop("disabled", false);
                    $("#rightpart").prop("disabled", false);
                }
                if (result.IsFlowChartEnable) {
                    $("#FlowChart").show();
                    $(".flowchart-div").show();
                }
                if (result.IsDocumentCodeEnabled) {
                    $(".doc-code").show();
                }
                if (result.IsFeaturedEnabled) {
                    $(".doc-feature").show();
                }
                if (result.Unlockdoc == true && !result.IsOnFileDeletion) {
                    $(".unlock-area").show();
                }
                if (result.IsApprover == true || result.IsAdmin || result.IsSmAdmin) {
                    if (result.IsApprovePopupEnabled)
                        $(".popup-approve-li").show();
                    else $(".approved-li").show();
                }
                if (result.IsPublished == false && result.HideApproveRejectButton == false && result.IsApprover)
                    $(".reject-approve-li").show();
                var ModeList = ["onapprove", "onreview", "reviewsubmit", "reviewerdraft", "approvesubmit"];
                if (((result.IsEditor == true || result.IsAdmin || result.IsSmAdmin)  && (ModeList.indexOf(result.PublishMode.toLowerCase()) < 0)) ||
                    ((result.IsReviewer == true || result.IsAdmin || result.IsSmAdmin) && (result.PublishMode == "onreview" || result.PublishMode == "reviewsubmit" || result.PublishMode == "reviewerdraft" || result.PublishMode == "onedit"))) {
                    $(".review-area-li").show();
                    var li = '';
            if ((result.IsEditor == true || result.IsAdmin || result.IsSmAdmin) && (ModeList.indexOf(result.PublishMode.toLowerCase()) < 0))
                        li += '<li>' +
                            '<a class="dropdown-item" href="javascript:void(0)" id="ReviewBtnTgle"><i class="fa fa-file-text-o" aria-hidden="true"></i> Submit for Review</a>' +
                            '</li>';
            if ((result.IsReviewer == true || result.IsAdmin || result.IsSmAdmin) || (result.PublishMode == "onreview" || result.PublishMode == "reviewsubmit" || result.PublishMode == "reviewerdraft" || result.PublishMode == "onedit"))
                        li += '<li>' +
                            '<a class="dropdown-item" href="javascript:void(0)" id="ApproverBtnTgle"><i class="fa fa-check-circle-o" aria-hidden="true"></i> Submit for Approve</a>' +
                            '</li>';
                    $(".review-area-li ul").html(li);
                }

        if ((result.IsReviewer == true || result.IsAdmin || result.IsSmAdmin) && (result.PublishMode == "onreview" || result.PublishMode == "reviewsubmit" || result.PublishMode == "reviewerdraft" || result.PublishMode == "onedit")) {
                    if (result.IsPublished == false && result.HideReviewRejectButton == false) {
                        $(".reject-doc-li").show();
                    }
                }

                WikiDocSettings.EditDoc._BindEditButtons();
            }
        });
    },
    _GetPrintSettings: function () {
        if (!$(".documentFields").hasClass("set-settings")) {
            var url = $("#hfdUrlPath").val() + '/DocumentSettings/GetPrintSettings';
            $.ajax({
                url: url,
                type: 'GET',
                data: { 'id': $("#Id").val() },
                crossDomain: true,
                success: function (result) {
                    $(".documentFields #pageSize").val(result.PaperSize == 1 ? "A5" : "A4");
                    $(".documentFields #waterMark").val(result.WaterMark);
                    $(".documentFields #marginLeft").val(result.MarginLeft);
                    $(".documentFields #marginRight").val(result.MarginRight);
                    $(".documentFields #marginTop").val(result.MarginTop);
                    $(".documentFields #marginBottom").val(result.MarginBottom);
                    $(".documentFields #IsbookletMode").prop('checked', result.IsBooklet);
                    $(".documentFields").addClass("set-settings");
                }
            });
        }
    },
    _BindEditButtons: function () {

        if ($('.link-box').is(":visible")) {
            $('.link-box').hide()
        }
        $("#ReviewBtnTgle").click(function () {
 // $("#ReviewBtnTgle").toggleClass("Submit-action");
            $('#EditorListErrorMessage').addClass('hidden');

            GetReviewerList();

        });

        $("#RejectApproveBtnTgle").click(function () {
            $("#RejectApprovePop").removeClass("hidden");
            $('#RejectApprovePop .pop-overlay').css('display', 'block');

            $("#RejectReason").removeClass("hidden");

        });

        $("#RejectReviewBtnTgle").click(function () {

            $("#RejectReviewPop").removeClass("hidden");
            $('#RejectReviewPop .pop-overlay').css('display', 'block');

            $("#RejectReviewReason").removeClass("hidden");

        });

        $("#ApproverBtnTgle").click(function () {
            $("#ReviewerListErrorMessage").addClass('hidden');

            GetApproverList();

            $("#RejectReviewReason").val('');

        });
        if ($("#ISFlowChart_Applicable").val() == "Y") {
            $(".flowchart-div").on("click", function () {
                init();
                $(".flowchart-div").off("click");
            });
        }

    },
    GetDraftDetails: function (id) {
        
        $.ajax({
            url: $("#hfdUrlPath").val() + '/DocumentSettings/GetDraftDetails',
            type: 'GET',
            data: { 'id': id },
            success: function (result) {
                if (result.IsFlowChartEnable) {
                    $("#FlowChart").show();
                    $(".flowchart-div").show();
                }
                $("#IsDocumentNumberIdentity").val(result.IsDocumentNumberIdentity);
                $("#IsFeaturedEnabled").val(result.IsFeaturedEnabled);
                $("#IsRecommendedEnabled").val(result.IsRecommendedEnabled);

                if (result.IsDocumentCodeEnabled) {
                    $(".doc-code").show();
                }
                if (result.IsFeaturedEnabled) {
                    $(".doc-feature").show();
                }

                if (result.IsAdmin && $("#IsFileDelete").val() != "true") {
                    $("#makeTemplate").show();
                }
                if (result.IsApprover == true) {
                    $(".approved-li").show();
                }
                if (result.IsEditor == true || result.IsReviewer == true) {
                    $(".review-area-li").show();
                    var li = '';
                    if (result.IsEditor == true) {
                        li += '<li>' +
                            '<a class="dropdown-item" href="javascript:void(0)" id="ReviewBtnTgle"><i class="fa fa-file-text-o" aria-hidden="true"></i> Submit for Review</a>' +
                            '</li>';
                    }
                    if (result.IsReviewer == true) {
                        li += '<li>' +
                            '<a class="dropdown-item" href="javascript:void(0)" id="ApproverBtnTgle"><i class="fa fa-check-circle-o" aria-hidden="true"></i> Submit for Approve</a>' +
                            '</li>';
                    }
                    $(".review-area-li ul").html(li);
                }
                WikiDocSettings.EditDoc._BindEditButtons();
            }
        });

    }
}

function process(date) {
    var parts = date.split("/");
    return new Date(parts[2], parts[1] - 1, parts[0]);
}
function formatDate(date) {
    var d = new Date(date),
        month = '' + (d.getMonth() + 1),
        day = '' + d.getDate(),
        year = d.getFullYear();

    if (month.length < 2) month = '0' + month;
    if (day.length < 2) day = '0' + day;

    return [year, month, day].join('-');
}


function Warning() {
    criticalNotify('You may acknowledge once you reach the end of the document ', '', "Warning");
}



;
var count = 1;
var timer = "";

CommonFunc.DownloadPdf = {
    _downloadData: function (id, type, canDownloadLinks) {
        if (!($("#hdnAppEnv").val() == 'V')) {
            canDownloadLinks = type == "Document" ? $("#CanDownloadLink").prop('checked') : canDownloadLinks;
            canDownloadLinks = type == "Document" ? (($("#CanDownloadLink").prop('checked') && ($(".filetext").length > 0 || $(".fileresource").length > 0)) ? true : false) : canDownloadLinks;
        }
        StopIntervel();
        gauge.update(0);
        percentageUpdate();
        var formData = new FormData();
        formData.append('PageSize', $("#pageSize option:selected").val());
        formData.append('WaterMark', $("#waterMark").val());
        formData.append('MarginLeft', $("#marginLeft").val() || 0);
        formData.append('MarginRight', $("#marginRight").val() || 0);
        formData.append('MarginTop', $("#marginTop").val() || 0);
        formData.append('MarginBottom', $("#marginBottom").val() || 0);
        formData.append('IsBookletMode', $("#IsbookletMode").prop('checked') || 0);
        formData.append('Id', id || 0);
        formData.append('Type', type || 0);
        formData.append('CanDownloadLink', canDownloadLinks || 0);

        $("#CanDownloadLink").prop('checked', false);

	    var xhttp;
	    if (window.XMLHttpRequest) { xhttp = new XMLHttpRequest(); }//code for modern browsers} 
	    else { xhttp = new ActiveXObject("Microsoft.XMLHTTP"); }// code for IE6, IE5
       if (canDownloadLinks && !($("#hdnAppEnv").val() == 'V')) {
                    $('.custompopup').hide().removeClass('popup-open');
                    xhttp.open("POST", $("#hfdUrlPath").val() + "/Document/DownloadDocFromDirectoryWithLinks", true);
            xhttp.send(formData);
            if ($("#downloadTemplate").length > 0) {
                $("#downloadTemplate").prop('disabled', true);
            }
            if ($("#downloadViewMode").length > 0) {
                $("#downloadViewMode").prop('disabled', true);
            }
            criticalNotify("Notification will appear once your zip file is ready for downloading", "", "Success");
        } else {
            $('.custompopup').hide().removeClass('popup-open');
            xhttp.open("POST", $("#hfdUrlPath").val() + "/Document/DownloadDocFromDirectory", true);
            $(".progressing-loader").show();
            xhttp.onprogress = function (event) {
                //var percent = Math.round(event.loaded * 100 / event.total);
                //gauge.update(percent);
            };

            xhttp.onreadystatechange = function () {

                if (xhttp.readyState == 4) {
                    if (xhttp.status == 200) {
                        StopIntervel();
                        var d = $.Deferred();
                        setTimeout(function () {
                            gauge.update(100);
                            d.resolve(true);
                        }, 1000);

                        d.promise();
                        d.then(function () {
                            var fileName = xhttp.getResponseHeader('File-Name');
                            var blob = new Blob([xhttp.response], { type: "octet/stream" });
                            saveAs(blob, fileName);
                            setTimeout(function () {
                                $(".progressing-loader").hide();
                                gauge.update(0);
                            }, 1000);
                        });
                    }
                    else {
                        StopIntervel();
                        $(".progressing-loader").hide();
                        var win = window.open($("#hfdUrlPath").val() + "/Error/FileNotFound");
                        setTimeout(function () {
                            win.close();
                            gauge.update(0);
                        }, 1000);
                    }
                }

            };
            xhttp.responseType = "arraybuffer";
            xhttp.send(formData);
        }
        
    },
    _downloadByFolder: function (id) {
        $('.custompopup').hide().removeClass('popup-open');
        $(".progressing-loader").show();

        StopIntervel();
        //setTimeout(function () {
        //    gauge.update(0);
        //}, 100);

        gauge.update(0);
        percentageUpdate();

        var formData = new FormData();
        formData.append('CategoryId', id);

        var xhttp;
        if (window.XMLHttpRequest) { xhttp = new XMLHttpRequest(); }//code for modern browsers} 
        else { xhttp = new ActiveXObject("Microsoft.XMLHTTP"); }// code for IE6, IE5

        xhttp.open("POST", $("#hfdUrlPath").val() + "/Document/DownloadByFolder", true);

        xhttp.onprogress = function (event) {
            //var percent = Math.round(event.loaded * 100 / event.total);
            //gauge.update(percent);
        };

        xhttp.onreadystatechange = function () {

            if (xhttp.readyState == 4) {
                if (xhttp.response.byteLength == 0) {
                    StopIntervel();
                    $(".progressing-loader").hide();
                    criticalNotify("No documents or files available!!", "", "Error");
                } else {
                    if (xhttp.status == 200) {
                        StopIntervel();
                        var d = $.Deferred();
                        setTimeout(function () {
                            gauge.update(100);
                            d.resolve(true);
                        }, 1000);

                        d.promise();
                        d.then(function () {
                            var fileName = xhttp.getResponseHeader('File-Name');
                            var blob = new Blob([xhttp.response], { type: "octet/stream" });

                            saveAs(blob, fileName);
                            setTimeout(function () {
                                $(".progressing-loader").hide();
                                gauge.update(0);
                            }, 1000);
                        });
                    }
                    else {
                        StopIntervel();
                        $(".progressing-loader").hide();
                        var win = window.open($("#hfdUrlPath").val() + "/Error/FileNotFound");
                        setTimeout(function () {
                            win.close();
                            gauge.update(0);
                        }, 1000);
                    }
                }
            }

        };
        xhttp.responseType = "arraybuffer";
        xhttp.send(formData);
    },
    _downloadDataFromHtml: function (canDownloadLink) {
        var IsbookletMode = $("#IsbookletMode").prop('checked');
        var templateHtml = $("#tableDetails").clone();

        var tableList = $('#tableDetails').find("[data-column='column-table-layout']").toArray();

        for (var item of tableList) {

            while ($(item).find('li > ul, li > ol').first().length) {

                var theItem = $(item).find('li > ul, li > ol').first();
                var nextTo = $(theItem).parents().not('li').first().children().last();

                $(theItem[0].outerHTML).insertAfter(nextTo);

                $(theItem).remove();

            }

        }

        $('#resizeTable td').each(function () {
            var itemList = $(this).find('ol,ul').parent(':not(ol,ul,div)').children('ol,ul');            
            $(itemList).each(function () {
                var div = document.createElement('div');
                div.innerHTML = $(this)[0].outerHTML;
                $(this)[0].outerHTML = div.outerHTML;
            });
        });               

        templateHtml = $("#tableDetails").clone();

		var name = $("#templateAcName").text().trim() || $("#DocumentName").val().trim() || "template";
		var pageSize = $("#pageSize").val();
		var waterMark = $("#waterMark").val();
		var marginLeft = $("#marginLeft").val();
		var marginRight = $("#marginRight").val();
		var marginTop = $("#marginTop").val();
        var marginBottom = $("#marginBottom").val();
		if (marginLeft > 36 || marginRight > 36 || marginTop > 36 || marginBottom > 36) {
		    criticalNotify('margin should be maximum up to 36', '', "Error");
			return false;
		}
		if ($('#tableDragger').find(">tr:not('.pagebreak')").length==0 || $('#tableDragger').find(">tr").length==0)
		{
		    criticalNotify('Sorry,empty document can\'t be downloaded', '', "Error");
		    return false;
		}
		if ($('#tableDragger').find(".noselect").length) {
		    criticalNotify('Image control cannot be empty', '', "Error");
		    return false;
		}
		if ($('#tableDragger').find(".nofileselect").length) {
		    criticalNotify('File control cannot be empty', '', "Error");
		    return false;
        }

        CommonFunc.DownloadPdf._downloadPdf(templateHtml.html(), name, pageSize, waterMark, marginLeft, marginRight, marginTop, marginBottom, IsbookletMode, canDownloadLink);

	},
    _downloadPdf: function (text, name, pageSize, waterMark, marginLeft, marginRight, marginTop, marginBottom, IsbookletMode, canDownloadLink) {
       

        var formData = new FormData();
        formData.append('Id', $("#Id").val());
	    formData.append('TemplateHtml', text);
	    formData.append('DocumentName', name);
	    formData.append('PageSize', pageSize);
	    formData.append('WaterMark', waterMark);
	    formData.append('MarginLeft', marginLeft || 0);
	    formData.append('MarginRight', marginRight || 0);
	    formData.append('MarginTop', marginTop || 0);
	    formData.append('MarginBottom', marginBottom || 0);
        formData.append('IsBookletMode', IsbookletMode || 0);
        formData.append('Type', $("#PageViewMode").val());
        formData.append('CanDownloadLink', canDownloadLink || 0);

	    var xhttp;
	    if (window.XMLHttpRequest) { xhttp = new XMLHttpRequest(); }//code for modern browsers} 
	    else { xhttp = new ActiveXObject("Microsoft.XMLHTTP"); }// code for IE6, IE5
        if (canDownloadLink && !($("#hdnAppEnv").val() == 'V')) {
            xhttp.open("POST", $("#hfdUrlPath").val() + "/Document/DownloadDocAsPdfWithLinks", true);
            criticalNotify("Notification will appear once your zip file is ready for downloading", "", "Success");
            if ($("#downloadTemplate").length > 0) {
                $("#downloadTemplate").prop('disabled', true);
            }
            if ($("#downloadViewMode").length > 0) {
                $("#downloadViewMode").prop('disabled', true);
            }
        } else {
            $(".progressing-loader").show();
            percentageUpdate();
            xhttp.open("POST", $("#hfdUrlPath").val() + "/Document/DownloadDocAsPdf", true);
            xhttp.onprogress = function (event) {
                //var percent = Math.round(event.loaded * 100 / event.total);
                //gauge.update(percent);
            };

            xhttp.onreadystatechange = function () {

                if (xhttp.readyState == 4) {
                    if (xhttp.status == 200) {
                        StopIntervel();
                        var d = $.Deferred();
                        setTimeout(function () {
                            gauge.update(100);
                            d.resolve(true);
                        }, 1000);

                        d.promise();
                        d.then(function () {
                            var fileName = xhttp.getResponseHeader('File-Name');
                            var blob = new Blob([xhttp.response], { type: "octet/stream" });
                            //var fileName = fileName;
                            saveAs(blob, fileName);
                            setTimeout(function () {
                                $(".progressing-loader").hide();
                                gauge.update(0);
                            }, 1000);
                        });
                    }
                    else {
                        StopIntervel();
                        $(".progressing-loader").hide();
                        var win = window.open($("#hfdUrlPath").val() + "/Error/FileNotFound");
                        setTimeout(function () {
                            win.close();
                            gauge.update(0);
                        }, 1000);
                    }
                }

            };
            xhttp.responseType = "arraybuffer";
        }
	    xhttp.send(formData);
	},
    _setHeader: function (htmlObject, doc) {

		htmlObject.find(".headerTemplate").each(function () {
            $(this).find("#docNumb").text((doc.DocumentNumber != "") ? doc.DocumentNumber: "[XXXX]");
            $(this).find("#approver").html((doc.ApprovedBy != "") ? doc.ApprovedBy : doc.Approver);
			if (doc.Version != undefined)
                $(this).find(".revision").html(doc.Version);
            if (clientName === "MINERVA") {
                $(this).find("#revision").text(doc.Version);
                if (doc.date !== undefined)
                    $(this).find("#documentDate").text(doc.date.substring(0, doc.date.indexOf(' ')));
                else {
                    if (doc.Date !== undefined)
                        $(this).find("#documentDate").text(doc.Date.substring(0, doc.Date.indexOf(' ')));
                }
            }
		});
		
	},
    _setFooter: function (htmlObject, doc) {
		if (doc.DocumentNumber != "" && doc.DocumentNumber != undefined)
			htmlObject.find("#footer #docNum").text(doc.DocumentNumber);

        if (doc.Date != "" && doc.Date != undefined && clientName.toLowerCase()  !== "imcsm") {
                htmlObject.find("#footer #docDate").text(moment(doc.Date, "DD-MM-YYYY").format("DD-MM-YYYY"));
        }

		if (doc.Version != "" && doc.Version != undefined)
		    htmlObject.find("#footer #DocV").text(doc.Version);

		if (doc.Version != undefined)
            htmlObject.find("#footer .revision").html(doc.Version);
        if (clientName.toLowerCase() === "imcsm")///remove
            htmlObject.find("#footer .revision").html(0);
	},
    _SavePDfData: function (IsApproved) {
		var PrintPDF = {};
		var templateHtml = $("#tableDetails").clone();
		templateHtml = CommonFunc.DownloadPdf._GenerateHtml(templateHtml);

		var RevisionNo = parseFloat(parseInt($("#leftpart").val()) + "." + parseInt($("#rightpart").val()));
		if (isNaN(RevisionNo))
		    RevisionNo = "0";

		if (IsApproved) {
		    var existValue = parseInt($("#CheckVersionVal").val() || 0);
		    if (parseInt(RevisionNo) == existValue)
		        RevisionNo = parseInt(RevisionNo) + 1.0;
		}

		var doc = {
		    'Version': RevisionNo,
		    'DocumentNumber': $("#DocNumber").val()|| $("#InitDocumentNumber").val() 
		};
		CommonFunc.DownloadPdf._setHeader(templateHtml, doc);
		CommonFunc.DownloadPdf._setFooter(templateHtml, doc);

        PrintPDF.HeaderHeight = $("#tableDragger").find("#header").find(".draggable-item").attr("height");
        PrintPDF.FooterHeight = $("#tableDragger").find("#footer").find(".draggable-item").attr("height");
        PrintPDF.TemplateHtml = escape(templateHtml.html().replace(/&lt;/gi, "_LessthanOperator"));

		return PrintPDF;
	},
	_SaveArticlePDfData: function (table, id) {
        var PrintArticlePDF = {};
      	var templateHtml = $(table).clone();
      	    
            templateHtml.find("#header").removeAttr('style');
            templateHtml.find("#footer").removeAttr('style');
            templateHtml.find("#header").css('opacity','1');
            templateHtml.find("#footer").css('opacity','1');

	    templateHtml.find(".drg-rmv").show();
	    templateHtml.find(".drg-rmv").removeAttr('style');
	    templateHtml.find(".drg-rmv-settings").show();
	    templateHtml.find(".drg-rmv-settings").removeAttr('style');
	    templateHtml.find(".drg-rmv-toc").show();
	    templateHtml.find(".drg-rmv-toc").removeAttr('style');
	    templateHtml.find(".drg-rmv-settings-toc").show();
	    templateHtml.find(".drg-rmv-settings-toc").removeAttr('style');
	    templateHtml.find(".drg-rmv-copy").show();
	    templateHtml.find(".drg-rmv-copy").removeAttr('style');
	    templateHtml.find(".drg-rmv-settings-copy").show();
	    templateHtml.find(".drg-rmv-settings-copy").removeAttr('style');
	    templateHtml.find(".drg-rmv-toc-copy").show();
	    templateHtml.find(".drg-rmv-toc-copy").removeAttr('style');
	    templateHtml.find(".drg-rmv-settings-toc-copy").show();
	    templateHtml.find(".drg-rmv-settings-toc-copy").removeAttr('style');
	    templateHtml.find(".dt-img").show();
	    templateHtml.find(".dt-img").removeAttr('style');
	    templateHtml.find(".templateSettings").show();
	    templateHtml.find(".templateSettings").removeAttr('style');
	    templateHtml.find(".ui-resizable-handle").show();
	    templateHtml.find(".ui-resizable-handle").removeAttr('style');
	   
	    templateHtml.find(">tbody>tr.temp-tr:first").remove();
	    templateHtml.find(".toggle-article").remove();
        
	    //templateHtml.find(">tbody>tr:first").before(templateHtml.find(">tbody>.headerTemplate"));
	    
	    PrintArticlePDF.ArticleId = id
        PrintArticlePDF.TemplateHtml = $(templateHtml).find('tbody').html();

	    return PrintArticlePDF;
	},
	_SaveArticleAsPDF:function(table)
    {

        var trs=$(table).clone().find(">tbody>tr");
	        var PdfTemplateHtml = document.createElement('table');
	        var tbody = document.createElement('tbody');
	        tbody.id = "tableDragger";
	        PdfTemplateHtml.appendChild(tbody);
	        $(PdfTemplateHtml).find("#tableDragger").append(trs);

	        $(PdfTemplateHtml).find("[contenteditable]").removeAttr("contenteditable");
	        $(PdfTemplateHtml).find(".pagebreak").css("page-break-after", "always").html("<td></td>");
	        $(PdfTemplateHtml).children().find("table.draggable-item").addClass("wikidoctemplate");
	        $(PdfTemplateHtml).find("#imgcontainer").unwrap("div");
	        $(PdfTemplateHtml).find(".comment-box").remove();
	        $(PdfTemplateHtml).find("script").remove();

            //if ($(PdfTemplateHtml).find(".headerTemplate:first").attr("data-column") == "column-PageHeader-BSM") {
            //    var pagehead = $(PdfTemplateHtml).find(".headerTemplate:first").find(".header-text").text();
            //    $(PdfTemplateHtml).find(".headerTemplate:first").find(".insertMeidum").html("<div style=\"background:#007ac9;color: #fff; padding: 6px;\"><table style=\"width:100%;\"><tbody><tr><td class=\"page-heading\">" + pagehead + "</td><td class=\"page-headingBSM\">BSM</td></tr></tbody></table></div>");
            //}

            $(PdfTemplateHtml).find(".filetext").each(function () { if ($(this).attr("onclick").includes("RenderLinkDocView") || $(this).attr("onclick").includes("RenderLinkArticleView")) { $(this).parent().text($(this).parent().text()); $(this).remove(); } });

            $(PdfTemplateHtml).find(".SubHeader1").each(function () {
                if ($(this).closest("td").attr("style") == undefined) {
                    $(this).closest("td").closest("table.draggable-item").removeAttr("height");
                    $(this).removeAttr("style");
                }
            });
            $(PdfTemplateHtml).find(".subheaderText").each(function () {
                $(this).closest("td").closest("table.draggable-item").removeAttr("height");
            });

	        $(PdfTemplateHtml).find(".htmlpreview").each(function () {
	            $(this).closest(".draggable-item").html($(this).html());
	        });
	        $(PdfTemplateHtml).find(".toggle-article").remove();
	        $(PdfTemplateHtml).find(">tbody>tr.temp-tr:first").remove();
	      

	        $(PdfTemplateHtml).find(".headerTemplate").show();
	        $(PdfTemplateHtml).find(".footerTemplate").show();
	        //$(PdfTemplateHtml).find(">tbody>tr:first").before($(PdfTemplateHtml).find(">tbody>.headerTemplate"));

	        
	        $(PdfTemplateHtml).find("#tableDragger .section-class-one").each(function () {
	            if ($(this).find("ol").length) {
	                $(this).find("ol").attr("style", $(this).attr("style"));
	                $(this).removeAttr("style");
	            }
	            if ($(this).find("ul").length) {
	                $(this).find("ul").attr("style", $(this).attr("style"));
	                $(this).removeAttr("style");
	            }
	        });
	        $(PdfTemplateHtml).find("#tableDragger .singleCol-text").each(function () {
	            if ($(this).find("ol").length) {
	                $(this).find("ol").attr("style", $(this).attr("style"));
	                $(this).removeAttr("style");
	            }
	            if ($(this).find("ul").length) {
	                $(this).find("ul").attr("style", $(this).attr("style"));
	                $(this).removeAttr("style");
	            }
	        });

	        $(PdfTemplateHtml).find("#tableDragger").find("[data-column='column-completed-layout']").each(function () {
	            var html = '<table style=\"width:100%;\" class="completed-table"><tbody><tr><td class="box-td" align="right"> <span style="font-family:\'Wingdings 2\';font-size:25px">ï¿½</span><span style="font-family: \'Wingdings 2\';font-size:25px">ï¿½</span></td><td class="completed-td">Completed</td><td class="box-td"> <span style="font-family: \'Wingdings 2\';font-size:25px">ï¿½</span><span style="font-family: \'Wingdings 2\';font-size:25px">ï¿½</span></td></tr></tbody></table>';
	            $(this).find(".completed-svg").replaceWith(html);
	            $(this).find("table.draggable-item").attr("height", 27);
            });

            $(PdfTemplateHtml).find("table[id^=resizeTable]").each(function () {
                $(this).removeAttr("border");
                $(this).css("width", $(this).attr("actual-width"));
                $(this).find(".resizeCol").each(function () {
                    if ($(this).width() > 50)
                        $(this).css("width", $(this).attr("actual-width"));
                });
                $(this).find(".ui-resizable-handle").remove();

                var tdBorder = $(this).find(">tbody>tr>td:eq(0)").css("border-left") != undefined && $(this).find(">tbody>tr>td:eq(0)").css("border-left") != "" ? $(this).find(">tbody>tr>td:eq(0)").css("border-left").trim().split(' ')[0] : "0.1px";
                var border = $(this).attr("border") != undefined && $(this).attr("border") != "" ? $(this).attr("border") : tdBorder;
                var thBorder = $(this).find(">thead th:eq(0)").css("border-bottom") != undefined && $(this).find(">thead th:eq(0)").css("border-bottom") != "" ? $(this).find(">thead th:eq(0)").css("border-bottom").trim().split(' ')[0] : "0.1px";

                $(this).removeAttr("cellpadding");
                $(this).removeAttr("cellspacing");


                $(this).find("thead th").each(function () {
                    if ($(this).css("height") != undefined && $(this).css("height") != "")
                        $(this).attr("style", "width:" + $(this).css("width") + ";height:" + $(this).css("height"));
                    else $(this).attr("style", "width:" + $(this).css("width"));
                });

                $(this).find("thead th").css("border-top-width", thBorder != "0.1px" ? thBorder : border);
                $(this).find("thead th:first").css("border-left-width", thBorder != "0.1px" ? thBorder : border);
                $(this).find("thead th:not(:first)").css("border-left-width", "0.1px");
                $(this).find("thead th:last").css("border-right-width", thBorder != "0.1px" ? thBorder : border);
                $(this).find(".ui-resizable-handle").remove();

                $(this).find(">tbody>tr").each(function (tr, trs) {
                    var isFirstHiddenCell = false;
                    $(this).find(">td").each(function (i, n) {

                        var IsHiddenCell = $(this).css("border");
                        $(this).css("border", "");
                        $(this).attr("style", "width:" + $(this).css("width"));

                        if (IsHiddenCell != "none") {
                            $(this).css("border-top-width", tr == 0 ? thBorder : "0.1px");
                            $(this).css("border-left-width", (i == 0 ? border != "0.1px" ? border : "0.1px" : "0.1px"));
                            if (i == $(this).parent().find(">td").length - 1)
                                $(this).css("border-right-width", border != "0.1px" ? border : "0.1px");
                        }
                        else if (!isFirstHiddenCell) {
                            isFirstHiddenCell = true;
                            $(this).css("border-left-width", "0.1px");
                        }
                    });
                });
                $(this).find(">tbody>tr:last").find(">td").each(function () {
                    var IsHiddenCell = $(this).css("border");
                    if (IsHiddenCell != "none")
                        $(this).css("border-bottom-width", border != "0.1px" ? border : "0.1px");
                });

                if ($(this).closest(".draggable-item").closest("tr").prev().find("table.draggable-item>tbody").find(".ol-box").length) {
                    if ($(this).closest(".draggable-item").closest("tr").prev().find("table.draggable-item>tbody").find(".ol-box:last").nextAll().text().length == 0)
                        $(this).closest(".draggable-item").css("width", "96%");
                    $(this).closest(".insertMeidum").attr("style", "padding-top:-5px");
                }
                else if ($(this).closest(".draggable-item").closest("tr").prev().find("table.draggable-item>tbody").find(".list-control-table").length) {
                    if ($(this).closest(".draggable-item").closest("tr").prev().find("table.draggable-item>tbody").find(".list-control-table:last").nextAll().text().length == 0)
                        $(this).closest(".draggable-item").css("width", "96%");
                    $(this).closest(".insertMeidum").attr("style", "padding-top:-5px");
                }

                $(this).closest(".draggable-item").closest("tr").find(".sub-disc-list").each(function () {
                    $(this).removeClass("sub-disc-list");
                });
            });
	        $(PdfTemplateHtml).find(".policy-table").each(function () {
	            $(this).removeAttr("border");
	            $(this).find("thead th").css("border-top-width", "0.1px");
	            $(this).find("thead th").css("border-left-width", "0.1px");
	            $(this).find("thead th:last").css("border-right-width", "0.1px");
	            $(this).find(".ui-resizable-handle").remove();
	            $(this).find("tbody td").css("border-top-width", "0.1px");
	            $(this).find("tbody td").css("border-left-width", "0.1px");
	            $(this).find("tbody tr").each(function () {
	                $(this).find("td:last").css("border-right-width", "0.1px");
	            });
	            $(this).find("tbody tr:last").each(function () {
	                $(this).find("td").css("border-bottom-width", "0.1px");
	            });
	            $(this).find("tbody tr:eq(1)").find("td:first").css("border-bottom-width", "0.1px");
	        });
	        $(PdfTemplateHtml).find(".pcgridview").each(function () {
	            $(this).find("table").each(function () {
	                $(this).find("tbody th").css("border-top-width", "0.1px");
	                $(this).find("tbody th").css("border-left-width", "0.1px");
	                $(this).find("tbody td").css("border-top-width", "0.1px");
	                $(this).find("tbody td").css("border-left-width", "0.1px");
	                $(this).find("tbody tr").each(function () {
	                    $(this).find("td:last").css("border-right-width", "0.1px");
	                    $(this).find("th:last").css("border-right-width", "0.1px");
	                });

	                $(this).find("tbody tr:last").each(function () {
	                    $(this).find("td").css("border-bottom-width", "0.1px");
	                });
	            });
	        });
