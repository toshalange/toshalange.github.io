        if (PublishedDoc) // show DCRL popup
            $('#tableDragger').on("contextmenu", function (event) {                
                savedSel = null;
                savedSelActiveElement = null;
                event.preventDefault();
                var selection = getSelected();
                selection = $.trim(selection);

                if (selection != '' && ISArticleHeader() != "temp-tr") {                    

                    if (clientName == "FLEX") {
                        $('div.popup-dcrlcomment .fileattachbtnsec').remove();
                        $('.fileattachbtnsec #fileAttachment').remove();
                        $('.fileattachbtnsec a').remove();
                        $('.fileattachbtnsec label').remove();

                        $('<span class=\"d-flex fileattachbtnsec\"><input type=\"file\" id=\"fileAttachment\" onchange=\"NewMethod(this)\" style=\"display:none\" /><label class=\"displayName\" title=\"\">No file chosen</label><a href=\"javascript:void(0)\" class=\"fileattachbtn\"><label for=\"fileAttachment\" class=\"forUpload\"><i class=\"fa fa-paperclip fa-2x\" aria-hidden=\"true\"></i></label></a><button class=\"pull-right btn red-buttons\" id=\"DeclDoneComment\" onclick=\"DCRLComment.Func.AddDCRLCommentArray()\">Save Change Request</button></span>').insertAfter(".dcrl-comment-content");

                        $('div.popup-dcrlcomment').find('#fileAttachment').val("");
                        InputFile = new File([""], "filename");
                    }

                    if (clientName == "MAERSK") {

                        if (getCurrentSelection().includes("><li")) {
                            return false;
                        }

                        $('div.popup-dcrlcomment #radbtns').remove();
                        $("<span id=\"radbtns\"><span class=\"radio radio-danger\"><input type=\"radio\" name=\"Type\" id=\"ToDocMajor\"><label for=\"ToDocMajor\">Major</label></span><span class=\"radio radio-danger\"><input type=\"radio\" name=\"Type\" id=\"ToDocMinor\" checked=\"checked\"><label for=\"ToDocMinor\">Minor</label></span></span>").insertAfter(".dcrl-comment-content");

                        var RdmajorID = "Maj" + uniqueObjectId();
                        var RdminorID = "Min" + uniqueObjectId();

                        $('#ToDocMajor').attr('id', RdmajorID);
                        $('#' + RdmajorID).next('label').attr('for', RdmajorID);

                        $('#ToDocMinor').attr('id', RdminorID);
                        $('#' + RdminorID).next('label').attr('for', RdminorID);

                    }

                    $(".dcrl-comment-content").css("border", "");
                    $("div.popup-dcrlcomment").css("display", "block");
                    $("div.popup-dcrlcommentZ").css("display", "none");

                    if (event.clientY >= 300) {
                        $("div.popup-dcrlcomment").css("top", event.clientY - 300);
                    }
                    else {
                        $("div.popup-dcrlcomment").css("top", event.clientY - 50);
                    }

                    if (($("#draggableContainer").width() - event.clientX) > 300) {
                        $("div.popup-dcrlcomment").css("left", event.clientX);
                    }
                    else {
                        $("div.popup-dcrlcomment").css("left", "auto");
                        $("div.popup-dcrlcomment").css("right", "0px");
                    }

                    $(".dcrl-comment-content").val("");
                    $("#Popup_Comment_Id").val("");
                    saveSelection();
                } else {
                    $("div.popup-dcrlcomment").css("display", "none");
                }
            });


