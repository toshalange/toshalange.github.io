          //  var text = "<ul><li>Sites: ";
          //  $.each(sites, function (i, site) {
          //      text = text + "<div><span class=\"tag-item\">" + site + "</span></div>";
           // });
          //  text = text + "</li></ul>";
          //  $("#doc-sites").html(text);

          //  $("ul.documentFields .tiny-loader").remove();

       // } else { $(".tiny-loader").remove(); }
   // },
    AssignFolderPath: function (folderPath) {
        if (folderPath != undefined && folderPath != "") {
            $("ul.documentFields").append(LoaderHtml());
            $(".categoryToolbarTitle").text(folderPath.split('/')[folderPath.split('/').length - 1]);
            $("ul.documentFields .tiny-loader").remove();
        }
    },
    _bindEvents: function () {
        
        //Zoomin and Zoomout functionality in View Page.
        var currZoom = $("#tableDragger").css("zoom");

        if (currZoom == '1') currZoom = 100;

        $(".zoomIn").click(function () {
            if (currZoom < 300) {
                currZoom = currZoom + 25;
                $("#tableDragger").css("zoom", currZoom + "%");
            }
            $(".zoomOff").text(currZoom + "%");
        });
        //$(".zoomOff").click(function () {
        
        //    currZoom = 100;
        //    $("#tableDragger").css("zoom", currZoom + "%");
        //    $(".zoomOff").text(currZoom + "%");

        //});
        $(".zoomOut").click(function () {
            if (currZoom > 25) {
                currZoom = currZoom - 25;
                $("#tableDragger").css("zoom", currZoom + "%");
            }
            $(".zoomOff").text(currZoom + "%");
        });

        $("#editTemplate").on("click", function () {
            QDMS.Menu.ShowDraftRecommendedMenu();
            $(".control-sidebar-tabs2").addClass("show-approvel-data");
            $("#PendingTab").show();
            $("#RejectedTab").show();
            $("#EditedTab").show();
            window.stop();
            $("#editTemplate").attr("disabled", "disabled");
            $("#editloader").show();
            $.ajax({
                type: 'GET',
                url: $("#hfdUrlPath").val() + '/Document/CheckDocEditPermission',
                data: { 'DocId': $("#Id").val() },
                processData: true,
                beforeSend: function () {

                },
                success: function (result) {
                    $("#editloader").hide();
                    $("#editTemplate").removeAttr("disabled");

                    var GetVal = result.split('/');
                    if (GetVal[0] == "true") {

                        if (GetVal.length > 1) {

                            if (GetVal[1] == "") {
                                RenderDocEditView($("#Id").val());
                            }
                            else {
                                RenderManageEditView(GetVal[1])
                            }
                        }

                    }
                    else if (GetVal[0] == "File-Delete") {
                        if (GetVal[1] == "you")
                            criticalNotify("Document now under the file deletion process by you, Please visit File Management Page.", '', "Error");
                        else criticalNotify("Document now under the file deletion process by " + GetVal[1] + ", Please visit File Management Page.", '', "Error");
                    }

                    else {
                        var editorname = result.split('/');
                        if (editorname.length > 1) {
                            criticalNotify("This Document is locked for new version by " + editorname[1], '', "Error");
                        }
                        else {
                            criticalNotify("This Document is locked for new version by " + editorname[0], '', "Error");
                        }

                        return false;
                    }
                }
            });

        });
        $('#downloadViewMode').click(function (event) {
            event.stopImmediatePropagation();
            
            var id = $("#documentId").val();

            if (viewMode == "edit" || viewMode == "new") {
                CommonFunc.DownloadPdf._downloadDataFromHtml();
            }
            else if (id != undefined && id != "" && viewMode == "view") {
                if ($("#fileType").val() == "Document") {
                    $(".loader10").show();
                    CommonFunc.DownloadPdf._GetPrintSettings(id);
                    $("#CanDownloadLink").prop('checked', false);
                    if ($("#hdnAppEnv").val() == 'V') {
                        $('#CanDownloadLink').parent().parent().parent().parent().parent().hide();
                    }
                }
                else {
                    if ($("#hdnAppEnv").val() == 'V') {
                        CommonFunc.DownloadPdf._downloadData(id, $("#fileType").val(), true);
                    } else {
                        if ($(".fileresource").length > 0 || $(".filetext").length > 0) {
                            ConfirmYesOrNoAlert("Do you want to include linked Document/Articles/Files on download ?").then(function (answer) {
                                CommonFunc.DownloadPdf._downloadData(id, $("#fileType").val(), answer);
                            });
                        } else {
                            CommonFunc.DownloadPdf._downloadData(id, $("#fileType").val(), false);
                        }
                    }
                    

                }
            }
        });
       $("#bookmarkTemplate").click(function () {


            var documentId = $("#Id").val();

            if (newBookmark.length > 0) {
                //$.each(newBookmark, function (key, val) {
                newBookmark = newBookmark.filter(x => {
                    return !bookmarkStart.some(t => t.BookmarkId === x.BookmarkId);
                });
            }

            if (bookmarkBegin > bookmarkStart) {
                var bookmarksToDelete = bookmarkBegin.filter(x => {
                    return !bookmarkStart.some(t => t.BookmarkId === x.BookmarkId);
                });
                $.each(bookmarksToDelete, function (ind) {
                    DeleteBookmark(bookmarksToDelete[ind].BookmarkId, documentId);
                });
            }

            $.ajax({
                type: 'POST',
                url: $("#hfdUrlPath").val() + '/Document/AddBookmarks',
                data: {
                    //'bookmarks': bookmarks, 'documentId': documentId
                    'bookmarks': newBookmark, 'documentId': documentId
                },
                processData: true,
                beforeSend: function () {
                },
                success: function (result) {
                    criticalNotify('Bookmarks updated successfully', '', "Success");
                },
                error: loadJsonError,
                complete: function () {
                }
            });
        });
        $("#EmailThisDoc").on("click", function () {

            var id = $("#Id").val();
            QDMS.Email.SendMail(id);
            //return false;

        });

