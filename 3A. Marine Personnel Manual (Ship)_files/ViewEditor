                        '</ul>');
                    if (result.IsPublished == true) {
                        $(".documentFields .doc-date").html('<ul><li>Published on <span class="spnDocUTCDate">' + result.date + '</span></li></ul>');
                    }
                    //else {
                    //    $(".documentFields .doc-date").html('<ul><li>Created on <span>' + result.date + '</span></li></ul>');
                    //}
                    if (result.IsPublished == true) {
                        $(".documentFields .doc-approver").html(

                            '<ul>' +
                            '<li>' +
                            '<div id="approved_wrapper">' +
                            'Approved By : <span id="approverName" class="text_box1">'+result.ApprovedBy+'	</span>' +
                            '<input type="hidden" value="Director LPSQ" id="approvedBy" />' +
                            '</div></li></ul>');//Director LPSQ
                    }

                    WikiDocSettings.DocumentDeatils.AssignTag(result.Tags);
                    WikiDocSettings.DocumentDeatils.AssignGroup(result.Groups);
                    WikiDocSettings.DocumentDeatils.AssignSites(result.Sites);

                    //tsk:3616
                    //WikiDocSettings.DocumentDeatils.GetOfficeViewvers(result.OfficeViewverList);
                    //WikiDocSettings.DocumentDeatils.GetVesselViewvers(result._VesselVIewList);

                    WikiDocSettings.DocumentDeatils.AssignFolderPath(result.folderPath);
                    $("#documentId").val(id);
                    $("#DocName").val(result.DocumentName);
                    $("#Approver").val(result.Approver);
                    $("#DocNumber").val(result.DocumentNumber);
                    $("#docversion").val(result.NewVersion);
                    $("#Date").val(result.date);
                    $("#fileType").val("Document");
                    $("#DocumentType").val(result.DocumentType);

                    var doc = {
                        'DocumentNumber': $("#DocNumber").val(),
                        'DocumentName': $("#DocName").val(),
                        'ApprovedBy': $("#Approver").val(),
                        'Version': $("#docversion").val(),
                        'Date': $("#Date").val()
                    }
                    CommonFunc.DownloadPdf._setHeader($('#tableDragger'), doc);
                    CommonFunc.DownloadPdf._setFooter($('#tableDragger'), doc);
                    if (clientName != null && clientName == "MOL") {
                        var k = $(".headerTemplate").length;
                        for (i = 0; i <= k; i++) {
                            $("#tableDragger").find(".headerTemplate").eq(i).find("#docDate").text(result.date.split(' ')[0].toString("dd/mm/yyyy").split("/").join("-"));
                            if (_objlist.WikiApproverList.includes(result.ApprovedBy)) {
                                $("#tableDragger").find(".headerTemplate").eq(i).find("#approver").text("Akihiro Yamauchi");
                            }
                            else {
                                $("#tableDragger").find(".headerTemplate").eq(i).find("#approver").text(result.ApprovedBy);
                            }
                            $("#tableDragger").find(".headerTemplate").eq(i).find("#docNumb").text(result.DocumentNumber);
                        }
                    }
                   
                    
                    WikiDocSettings.DocumentDeatils._bindEvents();
                    WikiDocSettings.ViewDocActivites.CheckExistance(id, result.Version);
                    if (result.IsPublished == true) {
                        WikiDocSettings.ViewDocActivites.AddRecentActivityAsync(id, result.DocumentName);
                    }
                    if (result.IsPublished == true) {
                        var StartTimeInfo = new Date().getTime();
                        $("#AcknlgeStartTime").val(StartTimeInfo);
                    }

                    if (result.Isvisible === false) {
                        $('#dcrklCommentTemplate').hide();
                        $('#EmailThisDoc').hide();
                        $('#bookmarkTemplate').hide();
                        $('#editTemplate').hide();
                        $('#Acknlwdged').fadeOut();
                        // $('#Acknlwdge').hide();
                        $('#Acknlwdge').fadeOut();

                        $('#DcrlHistoryComments').hide();
                        $("#containerDcrl_link").hide();



                    }

                }

            }

        });
    },
    GetArticleDeatils: function (id) {
        
        $("ul.documentFields").append(LoaderHtml());
        $.ajax({
            url: $("#hfdUrlPath").val() + '/DocumentSettings/GetArticleDeatils',
            type: 'GET',
            data: { 'id': id },
            success: function (result) {
                
                $("ul.documentFields .tiny-loader").remove();
                WikiDocSettings.DocumentDeatils.AssignTag(result.Tags);
                WikiDocSettings.DocumentDeatils.AssignGroup(result.Groups);
                WikiDocSettings.DocumentDeatils.AssignSites(result.Sites);
                WikiDocSettings.DocumentDeatils.GetOfficeViewvers(result.OfficeViewverList);
                WikiDocSettings.DocumentDeatils.GetVesselViewvers(result._VesselVIewList);
                WikiDocSettings.DocumentDeatils.AssignFolderPath(result.folderPath);
                $("#DocumentType").val(result.DocumentType);
                var k = $(".headerTemplate").length;
                if (clientName != null && clientName == "MOL") {
                    if ($('#hdnGlobalApprover').val() !== '' && $('#hdnGlobalApprover').val() !== undefined) {
                        if (new Date($("#Date").val().split(' ')[0].toString("dd/mm/yyyy")) >= new Date($('#hdnGloblApprvrEffectiveDate').val().split(' ')[0].toString("dd/mm/yyyy")))
                            $('#tableDragger').find('.headerTemplate').find('#approver').text($('#hdnGlobalApprover').val());
                    }
                    else {
                        for (i = 0; i <= k; i++) {
                            $("#tableDragger").find(".headerTemplate").eq(i).find("#docDate").text(result.date.split(' ')[0].toString("dd/mm/yyyy").split("/").join("-"));


                            if (result.WikiApproverList.includes(result.ApprovedBy)) {
                                $("#tableDragger").find(".headerTemplate").eq(i).find("#approver").text("Akihiro Yamauchi");
                            }
                            else {
                                $("#tableDragger").find(".headerTemplate").eq(i).find("#approver").text(result.ApprovedBy);
                            }


                        }
                    }
                    setReleasebyName();

                }
                

            }
        });
    },
    AssignTag: function (tags) {
        if (tags != null && tags != undefined && tags.length > 0) {
            $("ul.documentFields").append(LoaderHtml());

            var text = "<ul><li><div>Tags: </div>";
            $.each(tags, function (i, tag) {
                text = text + "<div id=\"newtags\">" +
                    "<span class=\"tag-item\" title='" + tag.Name + "'>" + tag.Name + "</span>" +
                    "</div>"
            });
            text = text + "</li></ul>";
            $("#tagList").html(text);

            $("ul.documentFields .tiny-loader").remove();

        } else { $(".tiny-loader").remove(); }
    },
    AssignGroup: function (groups) {

        if (groups != null && groups != undefined && groups.length > 0) {
            $("ul.documentFields").append(LoaderHtml());
            var text = "<ul><li><div>Groups: </div>";
            $.each(groups, function (i, group) {
                text = text + "<div id=\"newtags\">" +
                    "<span class=\"tag-item\" title='" + group.GroupName + "'>" + group.GroupName + "</span>" +
                    "</div>"
            });
            text = text + "</li></ul>";
            $("#groupList").html(text);
            $("ul.documentFields .tiny-loader").remove();

        } else { $(".tiny-loader").remove(); }
    },
    GetOfficeViewvers: function (officeUserList) {
        if (officeUserList != null && officeUserList != undefined && officeUserList.length > 0) {
            $("ul.documentFields").append(LoaderHtml());

            var text = "<ul><li>No.of Users Viewed: <span class=\"field-tip\">" +
                officeUserList.length + "<ul class=\"tip-content\">";
            $.each(officeUserList, function (i, user) {
                text = text + " <li>" + user + "</li>";
            });
            text = text + " </ul></span></li></ul>";
            $("#officeViewvers").html(text);

            $("ul.documentFields .tiny-loader").remove();

        } else { $(".tiny-loader").remove(); }
    },
    GetVesselViewvers: function (vesselUserList) {
        if (vesselUserList != null && vesselUserList != undefined && vesselUserList.length > 0) {
            $("ul.documentFields").append(LoaderHtml());

            var text = "<ul><li>No. of Vessels Viewed: <span class=\"field-tip\">" +
                vesselUserList.length + "<ul class=\"tip-content\">";
            $.each(vesselUserList, function (i, user) {
                text = text + " <li>" + user + "</li>";
            });
            text = text + " </ul></span></li></ul>";
            $("#vesselViewvers").html(text);

            $("ul.documentFields .tiny-loader").remove();

        } else { $(".tiny-loader").remove(); }
    },
 AssignSites: function (sites) {
     
        if (sites !== null && sites !== undefined && sites.length > 0) {
          // $("ul.documentFields").append(LoaderHtml());

            var text = "<ul><li>Sites: ";
           $.each(sites, function (i,site) {
              //if (site.indexOf("@") !== -1)
           if(site.includes("@"))
           {
               var eventNames = site.split('@');
            for (var idx = 0; idx < eventNames.length; idx++)
             {
                if (eventNames[idx].includes("VesselType :-") || eventNames[idx].includes("Vessel List"))
                 {
                     //<div><span class="tag-item"><b>@eventNames[idx].Replace('-', ' ')</b></span></div>
                     text = text + "<div><span class=\"tag-item\">" + eventNames[idx].replace('-', ' ') + "</span></div>";
                 }
                 else
                 {
                    // <div><span class="tag-item">@eventNames[idx]</span></div>
                    text = text + "<div><span class=\"tag-item\">" + eventNames[idx] + "</span></div>";
                 }

                     }
                    }
                    else
                    {
               if (site.includes("Offices:-"))
                             {  text = text + "<div><span class=\"tag-item\">" + site + "</span></div>";}
                         else
                         {
                             text = text + "<div><span class=\"tag-item\">" + site + "</span></div>";
                         }
                   
                    }
                });
         
           text = text + "</li></ul>";
            $("#doc-sites").html(text);

            $("ul.documentFields .tiny-loader").remove();
          

        } else { $(".tiny-loader").remove(); }
    },
