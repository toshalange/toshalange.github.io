var GlobalDocumentNumber = 0;
var VisibleButtons = [];
function ShowTemplateDetailsContainer() {
    $("#TemplateDetailsContainer").show();
    
    $("#template-tools-tab .tab-1, .tab-3,.tab-4").removeClass("active");
    $("#template-tools-tab .tab-2").addClass("active");
    $("#menuFlag").val("0");
   
}


function HideTemplateDetailsContainer() {
    $("#TemplateDetailsContainer").hide();
}

function ShowDocumentDetailsContainer_view() {
    $("#DocumentDetailContainer").show();
    $("body").removeClass("docInfo-panel-close");
    $("#template-tools-tab .tab-2, .tab-3,.tab-4,.tab-I").removeClass("active");
    $("#template-tools-tab .tab-1").addClass("active");
    $("#menuFlag").val("0");
    $('.documentsection').animate({
        scrollTop: 0
    }, 'slow');
    
}

function HideDocumentIndexContainer() {
    $("#DocumentIndexContainer").hide();
}
function ShowDocumentIndexContainer() {
    $("#DocumentIndexContainer").show();
    $("body").removeClass("docInfo-panel-close");
    $("#template-tools-tab .tab-1,.tab-2, .tab-3,.tab-4").removeClass("active");
    $("#template-tools-tab .tab-I").addClass("active");
    $("#menuFlag").val("0");
    $('.documentsection').animate({
        scrollTop: 0
    }, 'slow');
    if ($("#template-tools-tab .tab-I").hasClass("loaded") == false) {
        $("#template-tools-tab .tab-I").addClass("loaded");
        GetDocumentIndex();
    }
}
function ShowDocumentDetailsContainer() {
    $("#DocumentDetailContainer").show();
    $("body").removeClass("docInfo-panel-close");
    $("#template-tools-tab .tab-2, .tab-3,.tab-4,.tab-I").removeClass("active");
    $("#template-tools-tab .tab-1").addClass("active");
    $("#menuFlag").val("0");
    $('.documentsection').animate({
        scrollTop: 0
    }, 'slow');
    if ($("#template-tools-tab .tab-1").hasClass("loaded") == false) {
        $("#template-tools-tab .tab-1").addClass("loaded");
        BindLastDocumentNumber();
        LoadRelBynames();
        PopulateCurrentCategory();
        BindCategoryList();
        
    }
}

function ShowDocArticleDetailsContainer_view() {

    $("#DocumentDetailContainer").show();
    $("body").removeClass("docInfo-panel-close");
    $("#template-tools-tab .tab-2, .tab-3,.tab-4").removeClass("active");
    $("#template-tools-tab .tab-1").addClass("active");
    $("#menuFlag").val("0");
    $('.documentsection').animate({
        scrollTop: 0
    }, 'slow');
     

}

function ShowDocArticleDetailsContainer() {
    
    $("#DocumentDetailContainer").show();
    $("body").removeClass("docInfo-panel-close");
    $("#template-tools-tab .tab-2, .tab-3,.tab-4").removeClass("active");
    $("#template-tools-tab .tab-1").addClass("active");
    $("#menuFlag").val("0");
    $('.documentsection').animate({
        scrollTop: 0
    }, 'slow');
    if ($("#template-tools-tab .tab-1").hasClass("loaded") == false) {
        $("#template-tools-tab .tab-1").addClass("loaded");
        var url = $("#hfdUrlPath").val() + '/categories/ListAllArticleCategory';
        $.ajax({
            url: url,
            type: 'POST',
            crossDomain: true,
            success: function (result) {
                $("#category_list").html(result);
                setTimeout(function () { setCategory(); }, 1200)
            }
        });
        LoadRelBynames();
    }
     
}


function HideDocumentDetailsContainer() {
    $("#DocumentDetailContainer").hide();
}

function ShowPrintingDetailsContainer() {
    $("#printingPreference").show();
    $("#template-tools-tab .tab-1, .tab-2, .tab-3").removeClass("active");
    $("#template-tools-tab .tab-4").addClass("active");
    $("#menuFlag").val("0");
    $('.documentsection').animate({
        scrollTop: 0
    }, 'slow');
}
function HidePrintingDetailsContainer() {
    $("#printingPreference").hide();
}
function ShowArticleDetailsContainer() {
    $("#ArticleDetailsContainer").show();
    $("#template-tools-tab .tab-2 , .tab-1,.tab-4").removeClass("active");
    $("#template-tools-tab .tab-3").addClass("active");
    $("#menuFlag").val("1");

}

function GetDocumentIndex() {
        $("#tableDetails tr").each(function () {

        if ($(this).attr('data-column') == "column-TOC-layout") {
            $($(this).find('tr')).find('.TocTitle').each(function () {
                $(this).attr('title', $(this).attr('toc-title'));
            });
            // $(this).find('.insertMeidum tbody>tr>th').remove();
            $("#docIndex").append($(this).find('tr').html());
            $("#docIndex").find('.insertMeidum tbody>tr>th').remove();
        }
    });
}
function HideArticleDetailsContainer() {
    $("#ArticleDetailsContainer").hide();
}

function BindCategoryList() {
  
    RenderSelectCategoryList();
}

function BindApproverList() {
    RenderSelectApproverList();
}

function BindTagList() {
    GetTopTags();
}

$('#DocumentNumber').keyup(function () {
    debugger
  
    $('#DocNospan').find('span.errortext').remove();
    $('#DocNospan').removeClass('errortext');

    if (VisibleButtons.includes("Submit")) {
        $("button#btnGroupDrop1").show();
    }
    if (VisibleButtons.includes("Approve") || VisibleButtons.includes("approved-li")) {
        $('.approved-li').show();        
        $('.approved-li *').show();        
    }
    if (VisibleButtons.includes("Save Document")) {
        $('button.save_Draft').show();
        $('button.save_Document').show();
    }    
    
    });

$('#DocumentName').on('keyup', function (e) {    
    $("#tableDragger #documentname").each(function () {
        $(this).text($("#DocumentName").val());
    });

    if ($("#DocumentName").val() != "") {
        $("#tableDragger .headerdocname-fk").each(function () {
            $(this).text($("#DocumentName").val());
        });
    }
    else {
        $("#tableDragger .headerdocname-fk").each(function () {
            $(this).text("[Document / Article Name]");
        });
    }
    if ($("#DynamicHeader").val() == "true") {
       
        if ($("#DocumentName").val() != "") {
            $("#tableDragger .headerdocname-temp").each(function () {
                $(this).text($("#DocumentName").val());
            });
        }
        else {
            $("#tableDragger .headerdocname-temp").each(function () {
                $(this).text("[Document / Article Name]");
            });
        }
    }
}); 

$('#ArticleName').on('keyup', function (e) {    
    $("#tableDragger #documentname").each(function () {
        $(this).text($("#ArticleName").val());
    });
});

$('#DocumentNumber').focusout(function () {
    $('#DocNospan').find('span.errortext').remove();
    $('#DocNospan').removeClass('errortext');
    debugger
    var DocumentNumber = $('#DocumentNumber').val();
    if (DocumentNumber == "") {
        $("#DocumentNumber").val(DocumentNumber);
    }
    if (!DocumentNumber.match(/^[a-zA-Z0-9\s.\\\_\-\/]+$/)) {
        if (!$('.DocumentNumber').find('span.errortext').length) {
            $('<span></span>', {
                class: 'errortext',
                text: 'Special characters except [/,.,_,-,\] are not allowed'
            })
                .insertAfter('#DocumentNumber');
        }
        $('#DocumentNumber').addClass('errorinput');
        $('.addButtoninput').hide();
        $("#btnGroupDrop1").hide();
        $('.save_Document').hide();
        $('.save_Draft').hide();
        $('.approved-li').hide();
        return false;
     

    }
    if (DocumentNumber.length>20) {
        if (!$('.DocumentNumber').find('span.errortext').length) {
            $('<span></span>', {
                class: 'errortext',
                text: 'Not allow string  more than 20 characters'
            })
                .insertAfter('#DocumentNumber');
        }
        $('#DocumentNumber').addClass('errorinput');
       // $("#category_list").addClass('disabled');
        $('.addButtoninput').hide();
        $("#btnGroupDrop1").hide();
        $('.save_Document').hide();
        $('.save_Draft').hide();
        $('.approved-li').hide();
        return false;


    }
  
    if ((DocumentNumber != '') && (DocumentNumber.match(/^[a-zA-Z0-9\s.\\\_\-\/]+$/))) {
        var documentNumber = $("#DocumentNumber").val();
        if (GlobalDocumentNumber != documentNumber) {
            var url = $("#hfdUrlPath").val() + '/Document/IsDocumentNumberExists';
            $.ajax({
                url: url,
                type: 'POST',
                data: { 'documentNumber': documentNumber },
                crossDomain: true,
                success: function (result) {
                    debugger

                    if (result == "True") {
                        if (!$('.DocumentNumber').find('span.errortext').length) {
                            $('<span></span>', {
                                class: 'errortext',
                                text: 'Document Number already exists',
                                style: 'color:red'
                            })
                                .insertAfter('#DocumentNumber');
                        }
                        //   $("li catlist").addClass('disabled');
                        $('.addButtoninput').hide();
                        $("#btnGroupDrop1").hide();
                        $('.save_Document').hide();
                        $('.save_Draft').hide();
                        $('.approved-li').hide();
                        return false;

                    }
                    else {
                        $('.DocumentNumber').find('span.errortext').remove();
                        $('#DocumentNumber').removeClass('errorinput');

                        if (VisibleButtons.includes("Submit")) {
                            $("button#btnGroupDrop1").show();
                        }
                        if (VisibleButtons.includes("Approve")) {
                            $('.approved-li').show();
                        }
                        if (VisibleButtons.includes("Save Document")) {
                            $('button.save_Draft').show();
                        }
                    }


                },
                error: function (result) {

                }
            });

        }
       
    }

});




function IsDocumentNumberExists() {
    debugger
    $('#DocNospan').find('span.errortext').remove();
    $('#DocNospan').removeClass('errortext');
    var documentNumber = $("#DocumentNumber").val();
    if (documentNumber.length > 20) {
        if (!$('.DocumentNumber').find('span.errortext').length) {
            $('<span></span>', {
                class: 'errortext',
                text: 'Not allow string  more than 20 characters'
            })
                .insertAfter('#DocumentNumber');
        }
        $("#category_list").addClass('disabled');
        return false;
        

    }

    if (!documentNumber.match(/^[a-zA-Z0-9\s.\\\_\-\/]+$/)) {
        if (!$('.DocumentNumber').find('span.errortext').length) {
            $('<span></span>', {
                class: 'errortext',
                text: 'Special characters except [/,.,_,-,\]  are not allowed'
            })
                .insertAfter('#DocumentNumber');
        }
        $('#DocumentNumber').addClass('errorinput');
        $("#category_list").addClass('disabled');
        return false;
    }
    else { 
      
    var url = $("#hfdUrlPath").val() + '/Document/IsDocumentNumberExists';
    $.ajax({
        url: url,
        type: 'POST',
        data: { 'documentNumber': documentNumber },
        crossDomain: true,
        success: function (result) {
            if (result == "True") {
                criticalNotify("Document Number  already exists.", '', "Error");
                //$("#category_list").addClass('disabled');
                return false;
            }
            else {
                $("#category_list").removeClass('disabled');
                return true;
            }


        }
    });
    }
}
function BindLastDocumentNumber() {
    debugger

    if ($("#ArticleNumber").val() || $("#DocNumber").val()) {
        GlobalDocumentNumber = $("#ArticleNumber").val() || $("#DocNumber").val();
    }
    else {
        GlobalDocumentNumber = $("#DocumentNumber").val();
    }
    
    if ($("#Id").val()=="") {
        debugger
        var url = $("#hfdUrlPath").val() + '/Document/GetDocumentNumber';

        $.ajax({
            url: url,
            type: 'POST',
            data: { 'documentNumber': null, 'IsPublished': true },
            crossDomain: true,
            success: function (result) {
                debugger
                $("#DocumentNumber").val(result);
                
            }
        });
    }
}
function HidedocumentNameValidationMsg() {
    $("#documentNameValidationMsg").hide();
}

function ShowdocumentNameValidationMsg() {
    $("#documentNameValidationMsg").show();
}

function ShowCategoryIdValidationMsg() {
    $("#categoryIdValidationMsg").show();
}

function HideCategoryIdValidationMsg() {
    $("#categoryIdValidationMsg").hide();
}

function HideAllValidationMsg()
{
    HidedocumentNameValidationMsg();
    HideCategoryIdValidationMsg();
}

function DocumentName_Validation_ShowError() {
    HideTemplateDetailsContainer();
    HidePrintingDetailsContainer();
    HideArticleDetailsContainer();
    ShowDocumentDetailsContainer();
    ShowdocumentNameValidationMsg();
}
function ArticleName_Validation_ShowError() {
    HideTemplateDetailsContainer();
    HidePrintingDetailsContainer();
    HideArticleDetailsContainer();
    ShowDocArticleDetailsContainer();
    ShowdocumentNameValidationMsg();
}

function DocumentName_Validation_HideError() {
    HidedocumentNameValidationMsg();
}

function CategoryId_Validation_ShowError() {
    HideTemplateDetailsContainer();
    HideArticleDetailsContainer();
    ShowDocumentDetailsContainer();
    ShowCategoryIdValidationMsg();
    HidePrintingDetailsContainer();
}
function Article_CategoryId_Validation_ShowError() {
    debugger
    HideTemplateDetailsContainer();
    HideArticleDetailsContainer();
    ShowDocArticleDetailsContainer();
    ShowCategoryIdValidationMsg();
    HidePrintingDetailsContainer();
}

function CategoryId_Validation_HideError() {
    HideCategoryIdValidationMsg();
}

function PopulateCurrentCategory() {
    if ($("#CategoryId").val() != "") {
        BindSelectedCategory($("#CategoryId").val());
    }
    else {
        $("#CategoryName").text("Select Category");
    }
}

function SetCategory(Id) {
    $("#CategoryId").val(Id);
    BindSelectedCategory(Id);
    $("#Category_wrapper").toggleClass("drop-open");
}



function BindSelectedCategory(id) {
    debugger;
    if (id != undefined && id != "")
    {
        var url = $("#hfdUrlPath").val() + '/categories/GetCategory?Id=' + id;

        $.ajax({
            url: url,
            type: 'POST',
            crossDomain: true,
            success: function (result) {
                $("#CategoryName").text(result.Name);

                $("#tableDragger .headercategory-fk").each(function () {
                    $(this).text(result.Name);
                }); 
                if ($("#DynamicHeader").val() == "true") {
                    $("#tableDragger .headercategory-temp").each(function () {
                        $(this).text(result.Name);
                    });
                }
                $("#CategoryId").val(id)
                $("#newCategoryName").val($("#CategoryName").text());
                $(".categoryToolbarTitle").text(result.Name);
                $('.categoryToolbarTitle').attr('title', result.Name);
                $(".categoryToolbarTitle").attr('id', id);
            }
        });
    }
      
}

function clearEditableTr() {
    var dummyElement = $("#tableDragger tr");
    if (dummyElement.attr("data-placeholder") == "Type your text") {
        dummyElement.attr("contenteditable", "false");
        dummyElement.attr("data-placeholder", "");
    }
}
function LoadRelBynames() {
    if (clientName !== "MOL")
        return;
    $('#RelByName option:not(:first)').remove();
    var url = $("#hfdUrlPath").val() + '/Draft/GetRelByNamesList';
    $.ajax({
        url: url,
        type: 'GET',
        crossDomain: true,
        success: function (result) {
            $.each(result, function (index, value) {
                $("#RelByName").append($('<option>').text(value));
            });
            if ($('#hdnReleaseByName').val() !== '')
                $("#RelByName").val($('#hdnReleaseByName').val());
        }
    });
}

