                        }
                    }

                });
                
                // Acknowledgement Icon                
                if ($("#IsDocAckEnabled").val() == "True") {
                    if (($(this).scrollTop() + $(this).innerHeight() + 300) >= $(this)[0].scrollHeight) {
                        if ($("#IsSubmitAck").val() == "N") {

                            $('#Acknlwdge').fadeIn();
                            if ($('#Acknlwdge').attr("onclick") == "Warning()")
                                $('#Acknlwdge').attr("onclick", 'CheckAcknowledgementStatus()');
                            $('#Acknlwdged').fadeOut();
                        }

                    }
                    else {

                        //$('#Acknlwdge').fadeOut(); 

                        if ($("#IsSubmitAck").val() == "N") {
                            $('#Acknlwdge').fadeIn();
                            if ($('#Acknlwdge').attr("onclick") == "CheckAcknowledgementStatus()")
                                $('#Acknlwdge').attr("onclick", 'Warning()');
                            $('#Acknlwdged').fadeOut();
                        }
                    }
                }

            });

            try {
                if ($("#IsDocAckEnabled").val() == "True") {
                    setTimeout(function () {


                        if ($(".viewdoc").height() <= Math.round($(".viewdoc")[0].scrollHeight)) // for one page document without any scroll
                        {
                            if ($("#IsSubmitAck").val() == "N") {
                                $('#Acknlwdge').fadeIn();
                                $('#Acknlwdged').fadeOut();
                            } else {
                                $('#Acknlwdge').fadeOut();
