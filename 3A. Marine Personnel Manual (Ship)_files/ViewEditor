var WikiDocSettings = WikiDocSettings || {};
var newLoad = 0;

WikiDocSettings.DocumentDeatils = {

    GetDocDeatils: function (id) {

        $(".loadernew").remove();

        //$("ul.documentFields").append(LoaderHtml());
        $.ajax({
            url: $("#hfdUrlPath").val() + '/DocumentSettings/GetDocDeatils',
            type: 'GET',
            data: { 'id': id },
            success: function (result) {
                debugger
                var validity = true;
                if (result.PublishMode == "onreview" || result.PublishMode == "onapprove") {
                    if (result.ReviewerList != null) {
                        if (result.ReviewerList.length > 0 && result.ReviewerList.includes("Invalid"))
                            validity = false;
                    }
                    if (result.ApproverList != null) {
                        if (result.ApproverList.length > 0 && result.ApproverList.includes("Invalid"))
                            validity = false;
                    }
                }

                if (validity == true) {

                    if (result.Isvisible === false) {
                        result.IsAckSubmit = true;
                    }

                    if (result.IsAckSubmit == false && result.IsDocAckEnabled) {
                        
                        $('#Acknlwdge').fadeIn();
                        if ($('#Acknlwdge').attr("onclick") == "CheckAcknowledgementStatus()")
                            $('#Acknlwdge').attr("onclick", 'Warning()');
                        //$("#Acknlwdge").css("pointer-events", "none");
                    }
                    //else {
                    //    $('#Acknlwdged').fadeIn();
                    //    $("#Acknlwdge").css("pointer-events", "none");
                    //}

                    $(".control-sidebar-tabs2").addClass("show-approvel-data");
                    $(".show-approvel-data #RejectedTab").show();
                    $(".show-approvel-data #PendingTab").show();
                    $(".show-approvel-data #EditedTab").show();

                    $("#IsDCRLCycle").val(result.IsDCRLCycle);
                    //var date = formatDate(result.date);
                    //var BlockAckDate = formatDate(BlockAckDate.date);

                    //if (new Date(result.date.split(' ')[0]).getTime() <= new Date(result.BlockAckDate.split(' ')[0]).getTime()) {

                    if (result.IsDocAckEnabled) {
                        if (result.Isack == true) {
                            //if (result.date.split(' ')[0] <= result.BlockAckDate.split(' ')[0]) {
                            if (process(result.date.split(' ')[0]) <= process(result.BlockAckDate.split(' ')[0])) {



                                $("#IsSubmitAck").val("Y");
                                // if not in scroll
                                if (Math.round($(".viewdoc").innerHeight()) == Math.round($(".viewdoc")[0].scrollHeight)) {
                                    if ($("#IsSubmitAck").val() == "N") {
                                        $('#Acknlwdge').fadeIn();
                                        $('#Acknlwdged').fadeOut();
                                    }

                                }
                                else {
                                    $('#Acknlwdge').fadeOut();
                                    $('#Acknlwdged').fadeIn();
                                }

                            }
                            else {
                                // $("#IsSubmitAck").val("Y");

                                if (result.IsAckSubmit) {

                                    $("#IsSubmitAck").val("Y");
                                    //// if not in scroll
                                    //if ($(".viewdoc").innerHeight() == $(".viewdoc")[0].scrollHeight) {
                                    //    if ($("#IsSubmitAck").val() == "N") {
                                    //        $('#Acknlwdge').fadeIn();
                                    //        $('#Acknlwdged').fadeOut();
                                    //    }

                                    //}
                                    //else {
                                    $('#Acknlwdge').fadeOut();
                                    $('#Acknlwdged').fadeIn();
                                    //}
                                }

                                else {
                                    if (Math.round($(".viewdoc").innerHeight()) == Math.round($(".viewdoc")[0].scrollHeight)) {                                        
                                        if ($("#IsSubmitAck").val() == "N") {
                                            $('#Acknlwdge').fadeIn();
                                            if ($('#Acknlwdge').attr("onclick") == "Warning()")
                                                $('#Acknlwdge').attr("onclick", 'CheckAcknowledgementStatus()');
                                            $('#Acknlwdged').fadeOut();
                                        }

                                    }
                                }

                            }

                        }
                        else {


                            if (Math.round($(".viewdoc").innerHeight()) == Math.round($(".viewdoc")[0].scrollHeight)) {
                                if ($("#IsSubmitAck").val() == "N") {
                                    $('#Acknlwdge').fadeIn();
                                    //$("#Acknlwdge").css("pointer-events", "auto");
                                    if ($('#Acknlwdge').attr("onclick") == "Warning()")
                                        $('#Acknlwdge').attr("onclick", 'CheckAcknowledgementStatus()');
                                    $('#Acknlwdged').fadeOut();
                                }

                            }

                            if (result.IsAckSubmit) {

                                $("#IsSubmitAck").val("Y");
                                // if not in scroll
                                if (Math.round($(".viewdoc").innerHeight()) == Math.round($(".viewdoc")[0].scrollHeight)) {
                                    if ($("#IsSubmitAck").val() == "N") {
                                        $('#Acknlwdge').fadeIn();
                                        $('#Acknlwdged').fadeOut();
                                    }

                                }
                                else {
                                    $('#Acknlwdge').fadeOut();
                                    $('#Acknlwdged').fadeIn();
                                }
                            }


                        }
                    }

                    var Envset = $("#hdnAppEnv").val();
                    if (!result.IsPublished)
                        PublishedDoc = false;
                    else PublishedDoc = true;
                    if (result.IsPublished == false) {
                        $("#container_link").hide();
                        $('#Acknlwdge').fadeOut();
                        $('#Acknlwdged').fadeOut();
                        $("#containerbookmark_link").hide();

                        $("#containerDcrl_link").hide();
                    }
                    if (result.Isvisible) {
                        $("#HdnStatusFld").val("true");
                    }
                    else {
                        $("#HdnStatusFld").val("false")
                    }
                    if (result.Unlockdoc && !result.IsOnFileDeletion) {
