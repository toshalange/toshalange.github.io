            $.each($(this).find(">li"), function (i, n) {
                var innerText = $(this).find(":first-child").length ? CommonFunc.DownloadPdf._GetData($(this)) : $(this).contents()[0].data;
                text = text + "<tr><td width='" + firstTdWidth + "' style='" + padding + "'><font style=\"font-size: 10px;\">&#8211;</font></td><td width='" + secondTdWidth + "'>" + innerText + "</td></tr>";
                text = CommonFunc.DownloadPdf._TraverseList(text, $(this));
            });
            if (!$(this).hasClass("sub-hypen-list"))
                $(this).replaceWith(text + "</tbody></table>");
            else $(this).replaceWith(text + "</tbody></table></td></tr></tbody></table>");
        }
    });

        //TOC control
    var tocTable = templateHtml.find(".toc");
  
    if (tocTable.closest("table.draggable-item").closest("tr").next().hasClass("footerTemplate"))
        tocTable.closest("table.draggable-item").closest("tr").next().remove();

    if (tocTable.closest("table.draggable-item").closest("tr").next().hasClass("pagebreak"))
        tocTable.closest("table.draggable-item").closest("tr").next().remove();


    CommonFunc.DownloadPdf._removeUnwantedTags(templateHtml);
    return templateHtml;
    },
    _GetData: function (control) {
        var text = '<p>';
        control.contents().each(function (i,node)
        {
            if (node.nodeName != "UL" && node.nodeName != "OL") {
                if (node.nodeType == 1)
                    text = text + $(node).clone().wrap('<p>').parent().html();
                if (node.nodeType == 3)
                    text = text + node.textContent;
            }
        });
        return text+"</p>";
    },
    _GetPrintSettings: function (id)
    {
        $('#downloadTemplate').prop('disabled', true);
        var url = $("#hfdUrlPath").val() + '/DocumentSettings/GetPrintSettings';
        $.ajax({
            url: url,
            type: 'GET',
            data: { 'id': id},
            crossDomain: true,
            success: function (result) {
                
                $("#pageSize").val(result.PaperSize == 1 ? "A5" : "A4");
                if (result.PaperSize == 1) {
                    $("#marginTop").val(result.MarginTop != 25 ? result.MarginTop : "25");
                    $("#marginBottom").val(result.MarginBottom != 25 ? result.MarginBottom : "25");
                    $("#marginLeft").val(result.MarginLeft != 25 ? result.MarginLeft : "25");
                    $("#marginRight").val(result.MarginRight != 25 ? result.MarginRight : "25");
                }
                else {
                    $("#marginTop").val(result.MarginTop != 20 ? result.MarginTop : "20");
                    $("#marginBottom").val(result.MarginBottom != 20 ? result.MarginBottom : "20");
                    $("#marginLeft").val(result.MarginLeft != 30 ? result.MarginLeft : "30");
                    $("#marginRight").val(result.MarginRight != 30 ? result.MarginRight : "30");

                }
               $("#waterMark").val(result.WaterMark);
              // $("#marginLeft").val(result.MarginLeft);
               // $("#marginRight").val(result.MarginRight);
               // $("#marginTop").val(result.MarginTop);
               // $("#marginBottom").val(result.MarginBottom);
             
              $("#IsbookletMode").prop('checked', result.IsBooklet);
              $('.custompopup #docId').val(result.DocumentId);
              $('.custompopup').show().addClass('popup-open');
              $(".loader10").hide();
              $('#downloadTemplate').prop('disabled', false);
              $(".custompopup #pageSize").on('change', function () {
                  if ($("#pageSize option:selected").val() == "A4" )
                  {
                      if (result.PaperSize != 1) {
                          $("#marginTop").val(result.MarginTop != 20 ? result.MarginTop : "20");
                          $("#marginBottom").val(result.MarginBottom != 20 ? result.MarginBottom : "20");
                          $("#marginLeft").val(result.MarginLeft != 30 ? result.MarginLeft : "30");
                          $("#marginRight").val(result.MarginRight != 30 ? result.MarginRight : "30");
                      }
                      else {
                          $("#marginTop").val("20");
                          $("#marginBottom").val("20");
                          $("#marginLeft").val("30");
                          $("#marginRight").val("30");

                      }

                      
                      $(".custompopup #IsbookletMode").prop('checked', false);
                  }
               if ($("#pageSize option:selected").val() == "A5") {
                   if (result.PaperSize == 1) {
                       $("#marginTop").val(result.MarginTop != 25 ? result.MarginTop : "25");
                       $("#marginBottom").val(result.MarginBottom != 25 ? result.MarginBottom : "25");
                       $("#marginLeft").val(result.MarginLeft != 25 ? result.MarginLeft : "25");
                       $("#marginRight").val(result.MarginRight != 25 ? result.MarginRight : "25");
                   }
                   else {
                       $("#marginTop").val("25");
                       $("#marginBottom").val("25");
                       $("#marginLeft").val("25");
                       $("#marginRight").val("25");

                   }
                      $(".custompopup #IsbookletMode").prop('checked', true);
                  }
              });
            }
        });
    }
}
$(document).ready(function () {
    $('#downloadTemplate').click(function (event) {
        event.stopImmediatePropagation();

        var id = $("#documentId").val();
        
        if (viewMode == "edit" || viewMode == "new")
        {
            if ($(".fileresource").length > 0 || $(".filetext").length > 0) {
                ConfirmYesOrNoAlert("Do you want to include linked Document/Articles/Files on download ?").then(function (answer) {
                    CommonFunc.DownloadPdf._downloadDataFromHtml(answer);
                });
            } else {
                CommonFunc.DownloadPdf._downloadDataFromHtml(false);
            }
            
        }
        else if (id != undefined && id != "" && viewMode == "view")
        {
            if ($("#fileType").val() == "Document") {
                $(".loader10").show();
                CommonFunc.DownloadPdf._GetPrintSettings(id);
            }
            else {
                if ($("#hdnAppEnv").val() == 'V') {
                    CommonFunc.DownloadPdf._downloadData(id, $("#fileType").val(), true);
                } else {
                    if ($(".fileresource").length > 0 || $(".filetext").length > 0) {
                        ConfirmYesOrNoAlert("Do you want to include linked Document/Articles/Files on download ?").then(function (answer) {
                            CommonFunc.DownloadPdf._downloadData(id, $("#fileType").val(), answer);
                        });
                    } else {
                        CommonFunc.DownloadPdf._downloadData(id, $("#fileType").val(), false);
                    }
                }
            }
        }
    });
    $("#printpdf").click(function (event) {
        event.stopImmediatePropagation();
        CommonFunc.DownloadPdf._downloadData($('.custompopup #docId').val(),
            $("#fileType").val(), $('#CanDownloadLink').prop('checked'));
    });
});

function percentageUpdate() {
   
    var start = 0,
        end = 100;

    var range = end - start,
        current = start,
        increment = 1,
        stepTime = 1000;

    timer = setInterval(function () {
      
        if (current >= 100) {
            clearInterval(timer);
            current = 100;
            gauge.update(current);
        }
        else if (current < 99) {
            current += increment;
            gauge.update(current);
        }
        else if (current == 99) {
            gauge.update(current);
        }
    }, stepTime);
}
function StopIntervel()
{
    clearInterval(timer);
};
var text_nodes = "";
CommonFunc.Bookmark = {
