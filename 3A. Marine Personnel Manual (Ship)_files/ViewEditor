    $("#tableWrapper").on("click", '.add-comment', function (event) {        
        debugger;
        //var Ctrol_Tr_ID = $(this).closest(".bookmark-settings").closest("table").closest("tr").attr("id");
        var Ctrol_Tr_ID = "";
        if ($(this).closest(".bookmark-settings").closest("table").closest("tr").attr('class') == "checklist admin-checklist" || $(this).closest(".bookmark-settings").closest("table").closest("tr").attr('class') == "checklist")
            Ctrol_Tr_ID = $(this).closest(".bookmark-settings").closest("table").closest("tr").attr("uid");
        else if ($(this).closest(".bookmark-settings").closest("table").closest("tr").attr("tabindex") != null)
            Ctrol_Tr_ID = $(this).closest(".bookmark-settings").closest("table").closest("tr").attr("tabindex");
        else if ($(this).closest(".bookmark-settings").closest("table").closest("tr").attr("data-value") != null)
            Ctrol_Tr_ID = $(this).closest(".bookmark-settings").closest("table").closest("tr").attr("data-value");

        if (Ctrol_Tr_ID == "" || Ctrol_Tr_ID == undefined) {
            Ctrol_Tr_ID = $(this).closest("tr").attr("id");
        }

        if (clientName == "FLEX") {
            $('div.cntrlpopup-dcrlcomment .fileattachbtncntrl').remove();
            $('.fileattachbtnsec #fileAttachment').remove();
            $('.fileattachbtnsec a').remove();
            $('.fileattachbtnsec label').remove();

            if ($('#DCRStat').length > 0 && $('#DCRStat').val()) {
                $('<span class=\"d-flex fileattachbtncntrl\"><input type=\"file\" id=\"fileAtt\" onchange=\"ControlMethod(this)\" style = \"display:none\" /><label class=\"displayNameCntrl\" title=\"\">No file chosen</label><a href=\"javascript:void(0)\" class=\"fileattachbtnz\"><label for=\"fileAtt\" class=\"forUploadCntrl\"><i class=\"fa fa-paperclip fa-2x\" aria-hidden=\"true\"></i></label></a><button class=\"pull-right btn red-buttons\" id=\"Dcrl_control_DoneComment\" onclick=\"DraftDCRLComment.Func.DraftAdd_Control_DCRLCommentArray()\">Save Change Request</button></span >').insertAfter($('div.cntrlpopup-dcrlcomment .cntrldcrl-comment-content'));
            }
            else {
                $('<span class=\"d-flex fileattachbtncntrl\"><input type=\"file\" id=\"fileAtt\" onchange=\"ControlMethod(this)\" style = \"display:none\" /><label class=\"displayNameCntrl\" title=\"\">No file chosen</label><a href=\"javascript:void(0)\" class=\"fileattachbtnz\"><label for=\"fileAtt\" class=\"forUploadCntrl\"><i class=\"fa fa-paperclip fa-2x\" aria-hidden=\"true\"></i></label></a><button class=\"pull-right btn red-buttons\" id=\"Dcrl_control_DoneComment\" onclick=\"DCRLComment.Func.Add_Control_DCRLCommentArray()\">Save Change Request</button></span >').insertAfter($('div.cntrlpopup-dcrlcomment .cntrldcrl-comment-content'));
            }

            $('div.cntrlpopup-dcrlcomment').find('#fileAtt').val("");
            InputFile = new File([""], "filename");

            $("div.cntrlpopup-dcrlcomment").css("top", event.clientY - 20);

            $("div.cntrlpopup-dcrlcomment").css("left", event.clientX - 300);
            $("div.cntrlpopup-dcrlcomment").show();

            $("#Cntrl_Id").val(Ctrol_Tr_ID);
            $("#dcrl_content_area").css("border", "");
            $("#dcrl_content_area").val("");
        }
        else if (clientName == "MAERSK") {
            debugger;
            //$('div.popup-dcrlcomment #radbtns').remove();
            //$("<div id=\"radbtns\"><div class=\"radio radio-danger\"><input type=\"radio\" name=\"Type\" id=\"ToDocMajor\"><label for=\"ToDocMajor\">Major</label></div><div class=\"radio radio-danger\"><input type=\"radio\" name=\"Type\" id=\"ToDocMinor\" checked=\"checked\"><label for=\"ToDocMinor\">Minor</label></div></div>").insertAfter(".dcrl-comment-content");
            $("#Control_Popup").css("display", "block");
            $("#Control_Popup").css("top", event.clientY - 20);

            $("#Control_Popup").css("left", event.clientX - 300);
            var RdmajorID = "Maj" + uniqueObjectId();
            var RdminorID = "Min" + uniqueObjectId();

            $('#ToDocMajor').attr('id', RdmajorID);
            $('#' + RdmajorID).next('label').attr('for', RdmajorID);

            $('#ToDocMinor').attr('id', RdminorID);
            $('#' + RdminorID).next('label').attr('for', RdminorID);
            $("#Cntrl_Id").val(Ctrol_Tr_ID);
            if ($("#Control_Popup").find('.radio').length > 1) {
                $("#Control_Popup").find('.radio').eq(1).find('input').prop("checked", true)
                $("#Control_Popup").find('.radio').eq(0).find('input').attr('id', RdmajorID);
                $("#Control_Popup").find('.radio').eq(0).find('input').next('label').attr('for', RdmajorID)
                $("#Control_Popup").find('.radio').eq(1).find('input').attr('id', RdminorID);
                $("#Control_Popup").find('.radio').eq(1).find('input').next('label').attr('for', RdminorID)
            }
            $("#dcrl_content_area").css("border", "");
            $("#dcrl_content_area").val("");

        }
        else {
            $("#Control_Popup").css("display", "block");
            $("#Control_Popup").css("top", event.clientY - 20);

            $("#Control_Popup").css("left", event.clientX - 300);
            $("#Cntrl_Id").val(Ctrol_Tr_ID);
            $("#dcrl_content_area").css("border", "");
            $("#dcrl_content_area").val("");
             }
    });
    $('.remove-image').hide();
});


function TextContentChanges() {
    $(".doc-headername").focusout(function (event) {
        $(".loader10").show();
        $(this).closest(".headerTemplate").nextAll("tr").each(function () {
            $(this).find(".doc-headername").text(event.target.textContent);
        });
        $(".loader10").hide();
    });
    $(".header-num").focusout(function (event) {
        $(".loader10").show();
        $(this).closest(".headerTemplate").nextAll("tr").each(function () {
            $(this).find(".header-num").text(event.target.textContent);
        });
        $(".loader10").hide();
    });
    $(".header-docnum").focusout(function (event) {
        $(".loader10").show();
        $(this).closest(".headerTemplate").nextAll("tr").each(function () {
            $(this).find(".header-docnum").text(event.target.textContent);
        });
        $(".loader10").hide();
    });
    $(".approvedBy").focusout(function (event) {
        $(".loader10").show();
        $(this).closest(".headerTemplate").nextAll("tr").each(function () {
            $(this).find(".approvedBy").text(event.target.textContent);
        });
        $(".loader10").hide();
    });
    $(".effdate").focusout(function (event) {
        $(".loader10").show();
        $(this).closest(".headerTemplate").nextAll("tr").each(function () {
            $(this).find(".effdate").text(event.target.textContent);
        });
        $(".loader10").hide();
    });
    $(".site").focusout(function (event) {
        $(".loader10").show();
        $(this).closest(".headerTemplate").nextAll("tr").each(function () {
            $(this).find(".site").text(event.target.textContent);
        });
        $(".loader10").hide();
    });
    $(".headerTemplate .page-heading").focusout(function (event) {
        $(".loader10").show();
        $(this).closest(".headerTemplate").nextAll("tr").each(function () {
            $(this).find(".headerTemplate .page-heading").text(event.target.textContent);
        });
        $(".loader10").hide();
    });
    $(".bsm-header").focusout(function (event) {
        $(".loader10").show();
        $(this).closest(".headerTemplate").nextAll("tr").each(function () {
            $(this).find(".bsm-header").html(event.target.innerHTML);
        });
        $(".loader10").hide();
    });
    $(".option").focusout(function (event) {
        $(".loader10").show();
        $(this).closest(".headerTemplate").nextAll("tr").each(function () {
            $(this).find(".option").text(event.target.textContent);
        });
        $(".loader10").hide();
    });
    $('.header-text').focusout(function (e) {
        $(this).closest(".headerTemplate").parent().nextAll("tr").each(function () {
            if ($(this).html() !== $(e.target).html())
                $(this)[0].innerHTML = ($(e.target).html());
        });
    });
}

 
  var fileuploadHeightSpec = function () {
      var uploadcontainer = $('.file-body').height(),
      searcharea = $('#Sercharea').height() + $('#fileDataUpload').height(),
      uploadfileareaHeight = uploadcontainer - searcharea;

      $('#fileLoaded').css('height', uploadfileareaHeight - 150);
  }

WikiApp.fileTemplating = {
    _loadFileList: function (Keywordval, Categoryid) {
        var clientHeight = $(window).outerHeight();
        var tpBarHeight = $("#topbar").outerHeight(true);
        var logoHeader = $(".file-upload-btn").outerHeight(true);
        var hdrHeight = $("#file-list .sec-title").outerHeight(true);
        var wrapperHeight = parseInt(clientHeight - logoHeader - tpBarHeight - hdrHeight - 40);
        $('#fileList').html('');
        $.ajax({
            type: 'POST',
            url: $("#hfdUrlPath").val() + '/File/GetFiles',
            data: { Keyword: Keywordval, CategoryID: Categoryid },
            processData: true,
            success: function (result) {

                fileLoaded == true;
                if (result.errorType == "Error") {
                    criticalNotify("An Error has occured", "", "Error");
                    return;
                }
                $('#fileList').html(result);
                initLoadfileButton();
            }

        });
    },
    _loadFile: function (fileid) {
        $(".openFilePanel").on("click", function () {
            $("#file-list").removeClass("hide");
            $(".emailHd").addClass("hide");
            setTimeout(function () {
                $("#file-list").addClass("opening");
            }, 100);
        });
        if (fileObj == null) return;
        $.ajax({
            type: 'POST',
            url: $("#hfdUrlPath").val() + '/file/Showfile?id=' + fileid,
            //data: input,
            processData: true,
            beforeSend: function () {
            },
            success: function (result) {
                if (result.errorType == "Error") {
                    criticalNotify("An Error has occured", "", "Error");
                } else {
                    if (result != null) {
                        var resource = result;
                        var classEleName = "";
                        if (fileObj.context != undefined) {
                            classEleName = '.' + fileObj.context.className;

                            if (classEleName == '.thumb-file') {
                                $(fileObj).html('<a id="filecontainer" class="fileresource" href="#" onclick="downloadFileObject(\'' + resource.Id + '\')" data-fileid="' + resource.Id + '"  data-file-name="' + resource.DisplayName + resource.FileExtention + '">' + resource.DisplayName + '</a>').attr('data-fileid', resource.Id).attr('data-file-name', resource.DisplayName + resource.FileExtention);
                                fileObj.closest('#container-filewrap').find("#filewrap").removeClass("hide");
                                fileObj.css("height", $("#filecontainer").css("height"));
                                fileObj.closest(".draggable-item").css("height", $("#filecontainer").css("height"));
                                fileObj.closest(".draggable-item").closest("tr").css("height", $("#filecontainer").css("height"));
                                $(fileObj).unbind("click");
                            }
                            fileObj.closest(".thumb-wrapper").css("background-color", "#fff");
                        }
                        else {
                            $(fileObj[0]).html('<a id="filecontainer" class="fileresource" href="#" onclick="downloadFileObject(\'' + resource.Id + '\')" data-fileid="' + resource.Id + '"  data-file-name="' + resource.DisplayName + resource.FileExtention + '">' + resource.DisplayName + '</a>').attr('data-fileid', resource.Id).attr('data-file-name', resource.DisplayName + resource.FileExtention);
                            $(fileObj[0]).closest('#container-filewrap').find("#filewrap").removeClass("hide");
                            $(fileObj[0]).css("height", $("#filecontainer").css("height"));
                            $(fileObj[0]).closest(".draggable-item").css("height", $("#filecontainer").css("height"));
                            $(fileObj[0]).closest(".draggable-item").closest("tr").css("height", $("#filecontainer").css("height"));
                            $(fileObj[0]).unbind("click");

                            $(fileObj[0]).closest(".thumb-wrapper").css("background-color", "#fff");
                        }
                        $("#file-list").addClass("hide");
                        $(".emailHd").removeClass("hide");
                        //if (!$('.fileresource').find('span').length) {
                        //    $('<span></span>', { class: 'filetooltip', text: 'Download' }).appendTo('.fileresource');
                        //}
                        $('.fileresource').closest('.thumb-wrapper').css({ width: 'auto', minHeight: 'auto', border: '0', overflow: 'visible' })
                        $('.fileresource').hover(function () {
                            $(this).find('.filetooltip').stop().fadeIn(300);
                        }, function () {
                            $(this).find('.filetooltip').stop().fadeOut(300);
                        })

                        setTimeout(function () {
                            $("#file-list").removeClass("opening");
                        }, 100);
                    } else {

                    }
                }
            },
            error: loadJsonError,
            complete: function () {
            }
        });
    },
    _uploadFileForm: function () {

        $.ajax({
            type: 'GET',
            url: $("#hfdUrlPath").val() + '/file/AddFile',
            processData: true,
            beforeSend: function () {

            },
            success: function (result) {
                $("#fileUpload_Container").html(result);
                fileuploadHeightSpec();
            },
            error: loadJsonError,
            complete: function () {

            }
        });

    }
};

DocumentCommentOffice.Func = {
 
    PostComments: function () {        
        var DocID = $("#documentId").val();
        $('#tableDragger').find('grammarly-btn').remove() // Remove Gramamer tab from browser Extension
        if (DCRLCommentList_TR_Ids.length > 0 && DCRLCommentList_Details.length > 0) {
            $("#CommentTemplate").attr("disabled", "disabled")
            $("span.popup-updatedcrlcomment").css("display", "none");
            $("div.popup-dcrlcomment").css("display", "none");
            $("div.cntrlpopup-dcrlcomment").css("display", "none");
            var Savehtmldata = $("#tableDragger").html();
           
            var ArticleIDS = [];
            DCRLCommentList_ArticleDetails.filter(function (elem, pos) {
                if ($.inArray(elem[0], ArticleIDS) < 0) {
                    ArticleIDS.push(elem[0])
                     
                }
            });            
             
            $(".loader10").show();
            var encodedString = window.btoa(unescape(encodeURIComponent(Savehtmldata)));

            var formData = new FormData();            

            $.each(DCRLCommentList_Attach, function (i, item) {
                formData.append("Attachments", item);
            });
            $.each(DCRLCommentList_TR_Ids, function (i, item) {
                formData.append("TR_ID", item);
            });
            $.each(DCRLCommentList_Details, function (i, item) {
                formData.append("CommentListDetails", item);
            });
            $.each(ArticleIDS, function (i, item) {
                formData.append("ArticleListDetails", item);
            });
            $.each(DCRLCommentList_ParaId, function (i, item) {
                formData.append("Para_ID", item);
            });
            $.each(DCRLCommentList_Radio, function (i, item) {
                formData.append("IsMajorList", item);
            });

            formData.append('DocID', DocID);
            formData.append('DocHtml', encodedString);
                        
            $.ajax({
                type: 'POST',
                url: $("#hfdUrlPath").val() + '/Document/PostChangeRequest',
                
                processData: false,
                contentType: false,
                dataType: "json",
                async: true,                
                data: formData,                
                success: function (result) {
                    for (var i = 0; i < DCRLCommentList_TR_Ids.length; i++) {
                        DCRLComment.Func.ClearCommentAfterSave(DCRLCommentList_TR_Ids[i]);
                    }

                    DCRLCommentList_TR_Ids = [];
                    DCRLCommentList_Details = [];
                    DCRLCommentList_ArticleDetails = [];
                    DCRLCommentList_Attach = [];
                    $("#DCRLComments-list ul").html("");
                    //DocumentCommentList_TR_Ids_Old = [];
                    //DocumentCommentList_Details_Old = [];

                    $(".loader10").hide();

                    criticalNotify("Change Request Submited Successfully Please visit DCR List page for view comment status", "", "Success");
                    $("#CommentTemplate").removeAttr("disabled");
                    $("#DcrlHistoryComments").removeClass("hidden");
                },
                error: loadJsonError
            });
          

        }
        else {
            criticalNotify("No change request to save !", "", "Warning");
             
        }
    },
    SaveDraftPostComments: function () {    
        debugger;
        var DocID = $("#documentId").val();

        if (DCRLCommentList_TR_Ids.length > 0 && DCRLCommentList_Details.length > 0) {
            $("#CommentTemplate").attr("disabled", "disabled")
            $("span.popup-updatedcrlcomment").css("display", "none");
            $("div.popup-dcrlcomment").css("display", "none");
            $("div.cntrlpopup-dcrlcomment").css("display", "none");
            var Savehtmldata = $("#tableDragger").html();

            var ArticleIDS = [];
            DCRLCommentList_ArticleDetails.filter(function (elem, pos) {
                if ($.inArray(elem[0], ArticleIDS) < 0) {
                    ArticleIDS.push(elem[0])

                }
            });

            $(".loader10").show();
            var encodedString = window.btoa(unescape(encodeURIComponent(Savehtmldata)));

            var formData = new FormData();

            $.each(DCRLCommentList_Attach, function (i, item) {
                formData.append("Attachments", item);
            });
            $.each(DCRLCommentList_TR_Ids, function (i, item) {
                formData.append("TR_ID", item);
            });
            $.each(DCRLCommentList_Details, function (i, item) {
                formData.append("CommentListDetails", item);
            });
            $.each(ArticleIDS, function (i, item) {
                formData.append("ArticleListDetails", item);
            });
            $.each(DCRLCommentList_ParaId, function (i, item) {
                formData.append("Para_ID", item);
            });
            $.each(DCRLCommentList_Radio, function (i, item) {
                formData.append("IsMajorList", item);
            });

            formData.append('DocID', DocID);
            formData.append('DocHtml', encodedString);
                        
            $.ajax({
                type: 'POST',
                url: $("#hfdUrlPath").val() + '/Document/PostChangeDraftRequest',                
                processData: false,
                contentType: false,                
                dataType: "json",
                async: true,                
                data: formData,                
                success: function (result) {
                    for (var i = 0; i < DCRLCommentList_TR_Ids.length; i++) {
                        DCRLComment.Func.ClearCommentAfterSave(DCRLCommentList_TR_Ids[i]);
                    }

                    DCRLCommentList_TR_Ids = [];
                    DCRLCommentList_Details = [];
                    DCRLCommentList_ArticleDetails = [];
                    DCRLCommentList_Attach = [];
                    $("#DCRLComments-list ul").html("");
                    //DocumentCommentList_TR_Ids_Old = [];
                    //DocumentCommentList_Details_Old = [];

                    $(".loader10").hide();

                    criticalNotify("Change request save as draft successfully! ", "", "Success");
                    QDMS.Menu.GetUserRequestChangeDocList();
                  
                },
                error: loadJsonError
            });


        }
        else {
            criticalNotify("No change request to save !", "", "Warning");

        }
    },
     
}

function loadJsonError(e) {
    console.log(e);
}

//if you edit this function, please update OpenDocEditView() in index.js
function RenderDocEditView(id) {
    $("#IsFileDelete").val("false");
    //Rember same  this functon declared in Reuestchange.js "ProceedDocument"s please make any chaghes also their
    // same function definitin in index.js "RenderManageEditView" Method fro approval cycle functionality "Mnage document draft click"
    QDMS.Menu.ShowDraftRecommendedMenu();
    window.stop();
    //resetPercentageloader();
    //percentageLoader();
    $(".loader10").show();
    $(".loader10").css('z-index', '999');
    $("#contentWindow").html("");
    $("#container_link").hide();
    $('#Acknlwdge').fadeOut();
    $("#containerbookmark_link").hide();
    $("#containerDcrl_link").hide();
    $("#categoryMenu").hide();
    $(".search-form").addClass("hidden");
    $(".Searchexactphrase").addClass("hidden");
    if ($("#pageTab-container").find(".pageTab-item").length) {
        $("#pageTab-container").find(".pageTab-item").remove();
    }
    var url = $("#hfdUrlPath").val() + '/document/EditViewDoc?id=' + id;
    resetHistoryListView();
    resetBookmarkListView();
    $.ajax({
        url: url,
        type: 'Get',
        crossDomain: true,
        success: function (result) {
            $(".loader10").hide();
            $(".loader10").css('z-index', '');
            $("#contentWindow").html(result);
            $('#tableDragger').find('tr[data-column="column-table-layout"]').find('.templateSettings').empty();//Remove table settings layout inside table
            $('#tableDragger').find('.imgthspan').remove();//Remove add new column span 
            //$(".progress").hide();
            SetDocMode("edit");
            $(".two-column").find(".dt-img").css("display", "")
            PublishedDoc = false;
            $("body").removeClass();
            collapseSideBar();
            $("body").addClass("docInfo-panel-close");
            $("#template-tools-tab").find(".tab-1, .tab-2,.tab-3").removeClass("active");
            $(".toggle-article").removeClass("opentoggle");
            $("#IsDcrlReview").val("Y");

            if ($("#tableDragger >tr.flowchartDragger").length > 0) // have flow chart control read diagram from json value.
            {
                $("#tableDragger >tr.flowchartDragger").each(function () {
                    var TrID = $(this).find(".flow-chart-editor").attr("id");
                    if (TrID != "") {
                        // initalize the GoJs Diagram objectas globaly.
                        _FlowchartEditPanel(TrID);
                        window['myDiagram_' + TrID].model = go.Model.fromJson(document.getElementById("mySavedModel_" + TrID).value);

                    }


                });

            }
            if ($("#tableDragger .SubHeader1").length > 0) {
                $("#tableDragger .SubHeader1").each(function () {
                    if ($(this).find("span").html() === '') {
                    var divcontent = $(this).parent().children('div')[0].outerHTML
                        $(this).parent().children('div')[0].remove();
                        if ($(this).parent().children('p')[1].outerHTML === "<p></p>") {
                            $(this).parent().children('p')[1].remove();
                        }
                        $(this).find("span").append(divcontent);
                    }
                });
            }
            // reload article on scroll
            $(".tableDetails-wrapper").find(".article-table").attr("data-visible", false);

            setTimeout(function () {
                
                var doc = {
                    'DocumentNumber': $("#DocNumber").val(),
                    'DocumentName': $("#DocName").val(),
                    'ApprovedBy': $("#Approver").val(),
                    'Version': $("#docversion").val(),
                    'Date': $("#Date").val()
                };
                if (clientName.toLowerCase() === "imcsm") {//need to add (new condition)or revert this changeset
                    //$(".footerTemplate").find("#docDate").text(moment($('#Date').val().slice(0, $('#Date').val().indexOf(' ')), "DD-MMM-YYYY").format("DD-MMM-YYYY"));
                }

                var k = $(".headerTemplate").length;
                if (clientName !== null && clientName === "MOL") {
                    //var WikiApproverList = $("#hdnWikiApproverList").val();
                    for (i = 0; i <=k; i++) {

                      
                        //if (WikiApproverList.includes(doc.ApprovedBy)) {
                        //    $("#tableDragger").find(".headerTemplate").eq(i).find("#approver").text("Akihiro Yamauchi");
                        //}
                        //else {
                            $("#tableDragger").find(".headerTemplate").eq(i).find("#approver").text(result.ApprovedBy);
                        //}
                    }
                    //setGlobalApprover();
                }
                CommonFunc.DownloadPdf._setHeader($('#tableDragger'), doc);
                CommonFunc.DownloadPdf._setFooter($('#tableDragger'), doc);
                if (clientName !== null && clientName === "MOL")
                    setReleasebyName();
                $('img').on('dragstart', function (event) { event.preventDefault(); });
               // $("tr.draggable-item").find(".article-table>tbody>tr:not(:first)").addClass("hide-article");
                //$(".article-table").each(
                //    function (index) {
                //        ArticleData($(this));
                //    });


                var Artlcnt = $(".article-table").length;
                //if (Artlcnt > 3) {
                //    for (var i = 0; i <= 2; i++) {
                //        DocEditDCRLArticleData($(".article-table").eq(i), $("#IsDCRLCycle").val(), true, id);

                //    }
                //}
                //else {
                if (!isArticleDefaultHide) {
                    if (Artlcnt > 0) {
                        for (var i = 0; i <= Artlcnt; i++) {
                            DocEditDCRLArticleData($(".article-table").eq(i), $("#IsDCRLCycle").val(), true, id);
                        }
                    }
                }
               // }

                $("#tableWrapper").find('table').removeClass('NewDataArt');

                $(".toggle-article").click(function () {
                   
                    $(this).toggleClass('opentoggle'); 
                    if ($(this).closest("tr.draggable-item").find(".article-table").hasClass("NewDataArt") == false) {
                        var loaderNew = $('body').find('.loadernew')[0]
                        if (!$(this).closest("tr.draggable-item").find(".article-table").find(".loadernew").length)
                            $(this).closest("tr.draggable-item").find(".article-table").find('.insertMeidum h1.subheaderText').append(loaderNew)
                        DocEditDCRLArticleData($(this).closest("tr.draggable-item").find(".article-table"), $("#IsDCRLCycle").val(), true, id)
                    }

                });

                $(".article-table").each(function (coloumn, table) {
                	var Id = table.getAttribute("id");
                	$("#" + Id).each(function () {
                		$("#" + Id + " tr:not(:first-child)").find("tr").each(function (coloumn, tr) {
                			$(tr).find(".drg-rmv").html("");
                			$(tr).find(".drg-rmv-settings").html("");
                			$(tr).find(".bookmark-settings").hide();
                		});

                    });


                });

                // hide content of article except first 3 articles

                $('.toggle-article').each(function () {
                    if ($(this).closest("tr.draggable-item").find(".article-table").hasClass("NewDataArt") == false) {
                        var columnType = $(this).closest("tr.draggable-item").find(".article-table>tbody>tr:first").attr("data-column");
                        var text = $(this).closest("tr.draggable-item").find(".article-table").find(">tbody>tr:first").find(":header:first").text();
                        text = $(this).closest("tr.draggable-item").attr("toc-title") || text;

                        $(this).closest("tr.draggable-item").find(".article-table").addClass("hide-state");
                        $(this).closest("tr.draggable-item").find(".article-table>tbody>tr").addClass("hide-article");

                        if (columnType != "column-SubHeaderBSM-layout" && columnType != "column-ContentHeader-BSM" && columnType != "column-subheader1Layout-layout") {
                            $(this).closest("tr.draggable-item").find(".article-table>tbody>tr:first").before("<tr class=\"temp-tr\"><td><h2>" + text + "</h2></td></tr>");
                        }

                        else {
                            $(this).closest("tr.draggable-item").attr("toc-title", $(this).closest("tr.draggable-item").find(".article-table>tbody>tr:first").text().trim());
                            $(this).closest("tr.draggable-item").find(".article-table>tbody>tr:first").removeClass("hide-article");
                        }
                    }
                    //var ID = $(this).closest("tr.draggable-item").find(".article-table").attr("id");
                    //$(this).closest("tr.draggable-item").find(".article-table").removeClass("islockarticle");
                    //$("#" + ID).find('tr:not(.temp-tr)').remove();
                    //$("#" + ID).find('tbody').html(_articlesLoderHtmlTiny);
                });


            }, 100);

            setTimeout(function () {
                $('#tableDragger').find(".bookmark-settings").html("");
                $('#tableDragger').find(".bookmark-settings").hide();
                $('#tableDragger').find("#bookmark-list").find('.remove-bookmark').hide();

                $("#tableDragger").find('.two-column').each(function () {
                    if ($(this).find('.drg-rmv-settings-toc-copy').length === 0) {
                        if ($(this).find('td[data-two-col=two-column]').length > 0)
                            $(this).find('td[data-two-col=two-column]').next().after("<td style =\"width: 0;\" class=\"two-col-settings\"><div class=\"templateSettings\"></div><div class=\"bookmark-settings\"></div><span class=\"drg-rmv-settings-toc-copy\"> </span>");
                        else
                            $(this).find("td:last").after("<td style=\"width: 0;\" class=\"two-col-settings\"><div class=\"templateSettings\"></div><div class=\"bookmark-settings\"></div><span class=\"drg-rmv-settings-toc-copy\"> </span>");
                        $(this).find(".drg-rmv-settings-toc-copy").html(WikiApp.commonFunc._moveRemoveSettingsCopy());
                    }
                });
                RefreshView();
            });

        }
    });
}

function RefreshView() {
    $(".list-numbering-box").remove();

    $(".imgresource").resizable();
    $(".imgresource").resizable('destroy');
    $('.ui-wrapper').contents().unwrap();
    $(".thumb-img").find(".ui-resizable-handle").remove();
 
    $(".imgresource").closest(".ui-wrapper").attr("actual-width", $(".imgresource").css("width"));
    $(".imgresource").closest(".thumb-img").css("height", $(".imgresource").css("height"));
    $(".imgresource").closest(".thumb-img").css("width", $(".imgresource").css("width"));
    $(".ui-wrapper").nextAll(".ui-resizable-handle").remove();

    $(".two-column .imgresource").resizable('destroy');
    $(".logo-img .imgresource").resizable('destroy');
    $(".cover-layout .imgresource").resizable('destroy');

    $(".thumb-img .ui-wrapper").css("overflow", "visible");

    $("table[id^=resizeTable]").each(function () {
        $(this).resizable();
        $(this).resizable('destroy');
        $(this).find('.ui-wrapper').contents().unwrap();
        $(this).find(".thumb-wrapper").find(".ui-resizable-handle").remove();
        $(this).closest("table.draggable-item").find(".ui-resizable-handle").remove();
        $(this).resizable(
              {
                  containment: "#tableDragger",
                  stop: function (event, ui) {
                      ui.element.closest(".draggable-item").attr("height", ui.helper.outerHeight());
                      ui.element.closest(".draggable-item").find("#resizeTable").attr("actual-width", ui.element.closest(".draggable-item").find("#resizeTable").inlineStyle("width"));
                  }
              }
           );

        $(this).find(".resizeCol").resizable();
        $(this).find(".resizeCol").resizable('destroy');
        $(this).find('.ui-wrapper').contents().unwrap();
        $(this).find(".thumb-wrapper").find(".ui-resizable-handle").remove();
        $(this).find(".resizeCol").resizable({
            containment: "#resizeTable",
            stop: function (event, ui) {
                var totalWidth = 0;
                $(ui.helper).closest("#resizeTable").find("thead>tr>th").each(function () {
                    totalWidth = totalWidth + $(this).width();
                });
                if (totalWidth > $("#tableDragger").width())
                    $(ui.helper).closest("#resizeTable").find(".resizeCol").css("width", "50px");

                $(ui.helper).attr("actual-width", $(ui.helper).inlineStyle("width"));
            }
        });
        //if ($(this).attr("border").length && parseInt($(this).attr("border")) > 1) {
        //    $(this).find(".ui-resizable-s:last").css("border-bottom", parseInt($(this).attr("border")) + "px solid");
        //} else if ($(this).attr("border").length && parseInt($(this).attr("border")) ==1)
        //    $(this).find(".ui-resizable-s:last").css("border-bottom", "none");
    });

   
   //$(".title-class-one").contextMenu({
   //     menuSelector: "#formatcontextMenu",
   // });
   // $(".section-class-one").contextMenu({
   //     menuSelector: "#formatcontextMenu",
   // });
   // $(".title-class-two").contextMenu({
   //     menuSelector: "#formatcontextMenu",
   // });
   // $(".section-class-two").contextMenu({
   //     menuSelector: "#formatcontextMenu",
   // });
   // $(".toc-text").contextMenu({
   //     menuSelector: "#formatcontextMenu",
   // });
   // $(".singleCol-text").contextMenu({
   //     menuSelector: "#formatcontextMenu",
   // });

   // $(".ui-widget-text").contextMenu({
   //     menuSelector: "#formatcontextMenu",
   // });

    $(".comment-box").hide();

    $(".thumb-img").click(function () {

        imgLogoObj = $(this);

        setTimeout(function () {
            if (imageLoaded == false) {
                WikiApp.logoTemplating._loadImageList();
                WikiApp.logoTemplating._uploadImageForm();
                $("#logo-list").removeClass("hide");
                $(".emailHd").addClass("hide");
            }

            $("#logo-list").addClass("opening");
        }, 100);
    });
    $("#logorm").click(function () {
